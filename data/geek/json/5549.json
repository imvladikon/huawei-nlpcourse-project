13, \u05d4\u05e8\u05e6\u05dc\u05d9\u05d4 \u05e4\u05d9\u05ea\u05d5\u05d7\n\u05db\u05de\u05d4?\u00a0\u05d7\u05d9\u05e0\u05dd\n\u05d1-11 \u05dc\u05de\u05e8\u05e5, \u05d1\u05e9\u05e2\u05d4 17:30 \u05d9\u05d9\u05e2\u05e8\u05da \u05de\u05e4\u05d2\u05e9 \u05e2\u05dd \u05e9\u05d9 \u05d0\u05d2\u05e1\u05d9, \u05d1\u05d5 \u05d9\u05e9\u05ea\u05e3 \u05e9\u05d9 \u05de-25 \u05e9\u05e0\u05d5\u05ea \u05d4\u05e0\u05d9\u05e1\u05d9\u05d5\u05df \u05e9\u05dc\u05d5 \u05d1\u05e2\u05d5\u05dc\u05dd \u05d4\u05d9\u05d6\u05de\u05d5\u05ea \u05d5\u05d9\u05e1\u05d1\u05d9\u05e8 \u05db\u05d9\u05e6\u05d3 \u05dc\u05d2\u05d3\u05dc \u05e2\u05e1\u05e7\u05d9\u05dd \u05d2\u05d3\u05d5\u05dc\u05d9\u05dd.\n\u05dc\u05e4\u05e8\u05d8\u05d9\u05dd \u05e0\u05d5\u05e1\u05e4\u05d9\u05dd \u05d5\u05d4\u05e8\u05e9\u05de\u05d4.\n\u05e1\u05e4\u05d9\u05d3-\u05d3\u05d9\u05d9\u05d8\u05d9\u05e0\u05d2 \u05dc\u05de\u05e6\u05d9\u05d0\u05ea \u05e9\u05d5\u05ea\u05e4\u05d9\u05dd \u05dc\u05de\u05d9\u05d6\u05de\u05d9\u05dd #3 \u2013 \u05d9\u05d5\u05dd \u05e9\u05e0\u05d9, 19:00\u00a0\n\u05d0\u05d9\u05e4\u05d4?\u00a0\u05d1\u05e8 \u05d4\u05d1\u05d5\u05e8\u05dc\u05d0, \u05d9\u05e8\u05d5\u05e9\u05dc\u05d9\u05dd\n\u05db\u05de\u05d4?\u00a0\u05d7\u05d9\u05e0\u05dd\n\u05d1-11 \u05dc\u05de\u05e8\u05e5, \u05d1\u05e9\u05e2\u05d4 19:00 \u05d1\u05e2\u05e8\u05d1, \u05d1\u05de\u05d9\u05d3\u05d4 \u05d5\u05d9\u05e9 \u05dc\u05da \u05e8\u05e2\u05d9\u05d5\u05df \u05de\u05e2\u05d5\u05dc\u05d4 \u05dc\u05de\u05d9\u05d6\u05dd \u05d0\u05d1\u05dc \u05d7\u05e1\u05e8 \u05dc\u05da \u05d9\u05d3\u05e2 \u05d8\u05db\u05e0\u05d9? \u05d0\u05d5 \u05e9\u05d0\u05ea \u05de\u05ea\u05db\u05e0\u05ea\u05ea \u05e9\u05de\u05d7\u05e4\u05e9\u05ea \u05e9\u05d5\u05ea\u05e4\u05d9\u05dd \u05dc\u05d3\u05e8\u05da? \u05d5\u05d0\u05d5\u05dc\u05d9 \u05d0\u05ea\u05d4 \u05d3\u05d5\u05d5\u05e7\u05d0 \u05de\u05e2\u05e6\u05d1 \u05d0\u05d5 \u05d0\u05d9\u05e9 \u05e9\u05d9\u05d5\u05d5\u05e7 \u05e9\u05de\u05d7\u05e4\u05e9 \u05dc\u05d4\u05e6\u05d8\u05e8\u05e3 \u05dc\u05e1\u05d8\u05d0\u05e8\u05d8\u05d0\u05e4?\u00a0\u05d9\u05d6\u05de\u05d9\u05dd \u05e9\u05d9\u05e9 \u05dc\u05d4\u05dd \u05e8\u05e2\u05d9\u05d5\u05df \u05dc\u05de\u05d9\u05d6\u05dd \u05d9\u05e6\u05d9\u05d2\u05d5 \u05d0\u05ea \u05d4\u05de\u05d9\u05d6\u05dd \u05e9\u05dc\u05d4\u05dd \u05d1\u05ea\u05d7\u05d9\u05dc\u05ea \u05d4\u05e2\u05e8\u05d1, \u05db\u05d0\u05e9\u05e8 \u05db\u05dc \u05d9\u05d6\u05dd \u05d9\u05e7\u05d1\u05dc 2 \u05d3\u05e7\u05d5\u05ea \u05dc\u05d3\u05d1\u05e8 \u05d1\u05d4\u05df. \u05d4\u05d9\u05d6\u05de\u05d9\u05dd \u05e9\u05e8\u05d5\u05e6\u05d9\u05dd \u05dc\u05d4\u05e6\u05d8\u05e8\u05e3 \u05dc\u05e1\u05d8\u05d0\u05e8\u05d8\u05d0\u05e4 \u05d9\u05e9\u05de\u05e2\u05d5 \u05d0\u05ea \u05d4\u05e4\u05d9\u05e6\u2019\u05d9\u05dd \u05d5\u05d9\u05dc\u05db\u05d5 \u05dc\u05d3\u05d1\u05e8 \u05e2\u05dd \u05d4\u05d9\u05d6\u05de\u05d9\u05dd \u05e9\u05de\u05e2\u05e0\u05d9\u05d9\u05e0\u05d9\u05dd \u05d0\u05d5\u05ea\u05dd.\u00a0\u05de\u05e1\u05e4\u05e8 \u05d4\u05de\u05e7\u05d5\u05de\u05d5\u05ea \u05de\u05d5\u05d2\u05d1\u05dc.\n\u05dc\u05e4\u05e8\u05d8\u05d9\u05dd \u05e0\u05d5\u05e1\u05e4\u05d9\u05dd \u05d5\u05d4\u05e8\u05e9\u05de\u05d4.\n\u05d7\u05d3\u05e9\u05e0\u05d5\u05ea \u05d5\u05d0\u05d9\u05db\u05d5\u05ea \u05d1\u05e4\u05d9\u05ea\u05d5\u05d7 \u05ea\u05d5\u05db\u05e0\u05d4 \u2013 \u05d9\u05d5\u05dd \u05e9\u05dc\u05d9\u05e9\u05d9, 9:00\n\u05d0\u05d9\u05e4\u05d4?\u00a0\u05d4\u05ea\u05e2\u201d\u05e9 14 \u05db\u05e4\u05e8 \u05e1\u05d1\u05d0, \u05de\u05e8\u05db\u05d6 \u05d4\u05db\u05e0\u05e1\u05d9\u05dd 09\n\u05db\u05de\u05d4?\u00a0\u05d7\u05d9\u05e0\u05dd\n\u05d1-12 \u05dc\u05de\u05e8\u05e5, \u05d1\u05e9\u05e2\u05d4 9:00 \u05d1\u05d1\u05d5\u05e7\u05e8 \u05d9\u05d7\u05dc\u00a0\u05db\u05e0\u05e1 \u05d4\u05de\u05d9\u05d5\u05e2\u05d3 \u05dc\u05de\u05e4\u05ea\u05d7\u05d9\u05dd \u05d1\u05db\u05d9\u05e8\u05d9\u05dd, \u05e8\u05d0\u05e9\u05d9 \u05e6\u05d5\u05d5\u05ea\u05d9\u05dd, \u05de\u05e0\u05d4\u05dc\u05d9 \u05e4\u05d9\u05ea\u05d5\u05d7, \u05e1\u05de\u05e0\u05db\u05dc\u05d9 \u05e4\u05d9\u05ea\u05d5\u05d7, \u05