05d5\u05ea \u05d1\u05d0\u05e4\u05dc\u05d9\u05e7\u05e6\u05d9\u05d5\u05ea \u05de\u05e1\u05e8\u05d9\u05dd \u05db\u05de\u05d5 Whatsapp \u05d5-iMessage \u05d4\u05d9\u05d0 \u05d9\u05ea\u05e8\u05d5\u05df \u05d7\u05e9\u05d5\u05d1 \u05dc\u05de\u05e9\u05ea\u05de\u05e9\u05d9\u05dd, \u05e9\u05d9\u05db\u05d5\u05dc\u05d9\u05dd \u05dc\u05d9\u05d4\u05e0\u05d5\u05ea \u05de\u05de\u05d9\u05d3\u05ea \u05e4\u05e8\u05d8\u05d9\u05d5\u05ea \u05d4\u05e8\u05d1\u05d4 \u05d9\u05d5\u05ea\u05e8 \u05d2\u05d1\u05d5\u05d4\u05d4, \u05e8\u05e9\u05d5\u05d9\u05d5\u05ea \u05d4\u05d7\u05d5\u05e7 \u05de\u05ea\u05d9\u05d9\u05d7\u05e1\u05d5\u05ea \u05dc\u05e0\u05d5\u05e9\u05d0 \u05dc\u05e2\u05d9\u05ea\u05d9\u05dd \u05db\u05d0\u05d9\u05d5\u05dd \u05d1\u05d8\u05d7\u05d5\u05e0\u05d9 \u05e9\u05dc \u05de\u05de\u05e9. \u05d4\u05e4\u05e2\u05dd, \u05d4\u05d4\u05ea\u05e0\u05d2\u05d3\u05d5\u05ea \u05d4\u05db\u05dc\u05dc\u05d9\u05ea \u05e9\u05dc \u05de\u05de\u05e9\u05dc\u05d9\u05dd \u05dc\u05e0\u05d5\u05e9\u05d0 \u05e2\u05e9\u05d5\u05d9\u05d4 \u05dc\u05d4\u05ea\u05e8\u05d2\u05dd \u05d2\u05dd \u05dc\u05de\u05e2\u05e9\u05d9\u05dd, \u05db\u05d0\u05e9\u05e8 \u05d3\u05d9\u05d5\u05d5\u05d7 \u05d7\u05d3\u05e9 \u05d2\u05d5\u05e8\u05e1 \u05db\u05d9 \u05e6\u05d5\u05d5\u05ea \u05e9\u05dc \u05e0\u05e9\u05d9\u05d0 \u05d0\u05e8\u05e6\u05d5\u05ea \u05d4\u05d1\u05e8\u05d9\u05ea \u05d3\u05d5\u05e0\u05dc\u05d3 \u05d8\u05e8\u05d0\u05de\u05e4 \u05d3\u05df \u05d1\u05e6\u05e2\u05d3\u05d9 \u05d7\u05e7\u05d9\u05e7\u05d4 \u05db\u05e0\u05d2\u05d3 \u05d4\u05e6\u05e4\u05e0\u05d4.\n\u05e2\u05dc \u05e4\u05d9 \u05d3\u05d9\u05d5\u05d5\u05d7 \u05d1\u05d0\u05ea\u05e8 Politico, \u05e0\u05e4\u05d2\u05e9\u05d5 \u05d1\u05d9\u05d5\u05dd \u05d7\u05de\u05d9\u05e9\u05d9 \u05d4\u05d0\u05d7\u05e8\u05d5\u05df \u05d1\u05db\u05d9\u05e8\u05d9\u05dd \u05de\u05de\u05e9\u05dc \u05d8\u05e8\u05d0\u05de\u05e4 \u05e2\u05dd \u05d4\u05de\u05d5\u05e2\u05e6\u05d4 \u05dc\u05d1\u05d9\u05d8\u05d7\u05d5\u05df \u05dc\u05d0\u05d5\u05de\u05d9 \u05db\u05d3\u05d9 \u05dc\u05d3\u05d5\u05df \u05d1\u05e0\u05d5\u05e9\u05d0 \u05d4\u05d4\u05e6\u05e4\u05e0\u05d4 \u05d5\u05d3\u05e8\u05db\u05d9 \u05d4\u05d4\u05ea\u05de\u05d5\u05d3\u05d3\u05d5\u05ea \u05e9\u05dc \u05d4\u05de\u05de\u05e9\u05dc \u05e2\u05d9\u05de\u05d4. \u05d1\u05d9\u05df \u05d4\u05d9\u05ea\u05e8 \u05e2\u05dc\u05ea\u05d4 \u05d2\u05dd \u05d4\u05e6\u05e2\u05d4 \u05dc\u05d1\u05e7\u05e9 \u05de\u05d4\u05e7\u05d5\u05e0\u05d2\u05e8\u05e1 \u05dc\u05d0\u05e1\u05d5\u05e8 \u05d1\u05d7\u05d5\u05e7 \u05e2\u05dc \u05d7\u05d1\u05e8\u05d5\u05ea \u05d8\u05db\u05e0\u05d5\u05dc\u05d5\u05d2\u05d9\u05d4 \u05dc\u05d4\u05e9\u05ea\u05de\u05e9 \u05d1\u05e9\u05d9\u05d8\u05d5\u05ea \u05d4\u05e6\u05e4\u05e0\u05d4 \u05d1\u05dc\u05ea\u05d9 \u05e0\u05d9\u05ea\u05e0\u05d5\u05ea \u05dc\u05e4\u05d9\u05e6\u05d5\u05d7 \u05e2\u05dc \u05d9\u05d3\u05d9 \u05d2\u05d5\u05e8\u05de\u05d9 \u05d0\u05db\u05d9\u05e4\u05ea \u05d4\u05d7\u05d5\u05e7. \u05d4\u05e4\u05d2\u05d9\u05e9\u05d4 \u05d0\u05de\u05e0\u05dd \u05dc\u05d0 \u05d4\u05e1\u05ea\u05d9\u05d9\u05de\u05d4 \u05d1\u05d4\u05d7\u05dc\u05d8\u05d4 \u05e1\u05d5\u05e4\u05d9\u05ea \u05e2\u05dc \u05d3\u05e8\u05da \u05e4\u05e2\u05d5\u05dc\u05d4, \u05d0\u05da \u05d1\u05de\u05d4\u05dc\u05db\u05d4 \u05d4\u05d5\u05e6\u05e2\u05d4 \u05d3\u05e8\u05da \u05e0\u05d5\u05e1\u05e4\u05ea \u05dc\u05d4\u05ea\u05de\u05d5\u05d3\u05d3\u05d5\u05ea \u05d4\u05db\u05d5\u05dc\u05dc\u05ea \u05d4\u05e6\u05d4\u05e8\u05d4 \u05e4\u05d5\u05de\u05d1\u05d9\u05ea \u05d1\u05e0\u05d5\u05d2\u05e2 \u05dc\u05e1\u05dc\u05d9\u05d3\u05ea \u05d2\u05d5\u05e8\u05de\u05d9 \u05d4\u05d1\u05d9\u05d8\u05d7\u05d5\u05df \u05de\u05d4\u05e6\u05e4\u05e0\u05ea \u05e7\u05e6\u05d4-\u05dc\u05e7\u05e6\u05d4 \u05ea\u05d5\u05da \u05d4\u05de\u05e9\u05da \u05d7\u05d9\u05e4\u05d5\u05e9 \u05e4\u05ea\u05e8\u05d5\u05e0\u05d5\u05ea \u05d0\u05d7\u05e8\u05d9\u05dd \u05d1\u05de\u05e7\u05d1\u05d9\u05dc.\n\u05d4\u05e6\u05e4\u05e0\u05d4 \u05de\u05e7\u05e6\u05d4 \u05dc\u05e7\u05e6\u05d4 \u05d0\u05de\u05d5\u05e8\u05d4 \u05dc\u05d4\u05d1\u05d8\u05d9\u05d7 \u05e9\u05e