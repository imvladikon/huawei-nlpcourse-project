05d9\u05d9\u05dd \u05e2\u05d3 \u05e9\u05de\u05d2\u05d9\u05e2\u05d9\u05dd \u05dc\u05d0\u05de\u05df \u05e2\u05e6\u05de\u05d5, \u05d4\u05d5\u05d3\u05d5\u05ea \u05dc\u05d2\u05d5\u05e8\u05de\u05d9\u05dd \u05d4\u05e8\u05d1\u05d9\u05dd \u05d4\u05de\u05e2\u05d5\u05e8\u05d1\u05d9\u05dd \u05d1\u05db\u05da \u05e9\u05e9\u05d9\u05e8\u05d5 \u05d6\u05de\u05d9\u05df \u05dc\u05e8\u05db\u05d9\u05e9\u05d4 \u05d1\u05d0\u05d9\u05e0\u05d8\u05e8\u05e0\u05d8. \u05d1\u05de\u05d8\u05e8\u05d4 \u05dc\u05e6\u05de\u05e6\u05dd \u05d0\u05ea \u05d4\u05e4\u05e2\u05e8 \u05d1\u05d9\u05df \u05d4\u05d0\u05de\u05e0\u05d9\u05dd \u05dc\u05de\u05e2\u05e8\u05d9\u05e6\u05d9\u05dd, \u05d4\u05e9\u05d9\u05e7\u05d4 \u05d1\u05d9\u05d8\u05d5\u05e8\u05e0\u05d8 \u05dc\u05e4\u05e0\u05d9 \u05db\u05d7\u05de\u05d9\u05e9\u05d4 \u05d7\u05d5\u05d3\u05e9\u05d9\u05dd \u05d0\u05ea \u05d7\u05e0\u05d5\u05ea \u05d4\u05de\u05d3\u05d9\u05d4 \u05d4\u05d7\u05d3\u05e9\u05d4,\u00a0BitTorrent Bundle.\n\u05de\u05d3\u05d5\u05d1\u05e8 \u05d1\u05e7\u05d5\u05e0\u05e1\u05e4\u05d8 \u05e9\u05d5\u05e0\u05d4 \u05dc\u05d7\u05dc\u05d5\u05d8\u05d9\u05df \u05de\u05de\u05d4 \u05e9\u05d4\u05db\u05e8\u05e0\u05d5 \u05e2\u05d3 \u05d4\u05d9\u05d5\u05dd, \u05d1\u05de\u05d9\u05d5\u05d7\u05d3 \u05d1\u05ea\u05d7\u05d5\u05dd \u05d4\u05d8\u05d5\u05e8\u05e0\u05d8\u05d9\u05dd. \u05d1\u05e0\u05d9\u05d2\u05d5\u05d3 \u05dc\u05e9\u05d9\u05e8\u05d5\u05ea\u05d9\u05dd \u05db\u05d2\u05d5\u05df \u05d0\u05d9\u05d9\u05d8\u05d5\u05e0\u05e1 \u05d5\u05d3\u05d5\u05de\u05d9\u05d5, \u05db\u05d0\u05df \u05dc\u05d0 \u05de\u05d3\u05d5\u05d1\u05e8 \u05e2\u05dc \u05d4\u05d5\u05e8\u05d3\u05d4 \u05e9\u05dc \u05d0\u05dc\u05d1\u05d5\u05dd \u05e9\u05dc\u05dd, \u05de\u05e1\u05e4\u05e8 \u05e7\u05d1\u05e6\u05d9 \u05de\u05d3\u05d9\u05d4 \u05d0\u05d5 \u05d0\u05e4\u05d9\u05dc\u05d5 \u05e1\u05e8\u05d8\u05d5\u05e0\u05d9\u05dd, \u05d0\u05dc\u05d0 \u05e7\u05d5\u05d1\u05e5 \u05de\u05d5\u05dc\u05d8\u05d9\u05de\u05d3\u05d9\u05d4 \u05d4\u05de\u05d7\u05d6\u05d9\u05e7\u00a0\u05dc\u05e6\u05d3 \u05ea\u05db\u05e0\u05d9 \u05d4\u05de\u05d3\u05d9\u05d4 \u05d0\u05e4\u05e9\u05e8\u05d5\u05ea \u05d2\u05d9\u05e9\u05d4 \u05dc\u05ea\u05db\u05e0\u05d9 Premium, \u05e9\u05d9\u05e4\u05ea\u05d7\u05d5 \u05d1\u05d4\u05ea\u05d0\u05dd \u05dc\u05d4\u05d2\u05d3\u05e8\u05d5\u05ea\u05d9\u05d5 \u05e9\u05dc \u05d9\u05d5\u05e6\u05e8 \u05d4\u05d8\u05d5\u05e8\u05e0\u05d8 \u05d4\u05de\u05e7\u05d5\u05e8\u05d9.\n\u05d1\u05e4\u05d5\u05e2\u05dc, \u05dc\u05d0\u05d7\u05e8 \u05e9\u05d4\u05de\u05e9\u05ea\u05de\u05e9 \u05d9\u05d5\u05e8\u05d9\u05d3 \u05d0\u05ea \u05d4\u05d1\u05d0\u05e0\u05d3\u05dc \u05d4\u05e8\u05e6\u05d5\u05d9 \u05d9\u05d5\u05db\u05dc \u05dc\u05d4\u05d0\u05d6\u05d9\u05df \u05dc\u05d0\u05dc\u05d1\u05d5\u05dd \u05d0\u05d5 \u05dc\u05e9\u05d9\u05e8 \u05d4\u05de\u05dc\u05d0 \u05d1\u05d5 \u05d4\u05d5\u05d0 \u05d4\u05ea\u05e2\u05e0\u05d9\u05d9\u05df \u05de\u05dc\u05db\u05ea\u05d7\u05d9\u05dc\u05d4, \u05d5\u05ea\u05de\u05d5\u05e8\u05ea \u05de\u05d9\u05d3\u05e2 \u05e0\u05d5\u05e1\u05e3 \u05de\u05d4\u05e6\u05d3 \u05e9\u05dc\u05d5, \u05db\u05d2\u05d5\u05df \u05db\u05ea\u05d5\u05d1\u05ea \u05d4\u05de\u05d9\u05d9\u05dc \u05d0\u05d5 \u05ea\u05e9\u05dc\u05d5\u05dd, \u05d9\u05d5\u05db\u05dc \u05dc\u05e7\u05d1\u05dc \u05d2\u05d9\u05e9\u05d4 \u05dc\u05d4\u05d5\u05e8\u05d3\u05ea \u05e2\u05d5\u05d3 \u05e9\u05d9\u05e8\u05d9\u05dd, \u05e7\u05dc\u05d9\u05e4\u05d9\u05dd \u05d5\u05ea\u05db\u05e0\u05d9\u05dd \u2013 \u05d1\u05e6\u05d5\u05e8\u05d4 \u05e4\u05e9\u05d5\u05d8\u05d4, \u05de\u05d4\u05d9\u05e8\u05d4 \u05d5\u05db\u05de\u05d5\u05d1\u05df \u05d7\u05d5\u05e7\u05d9\u05ea.\n\u05d1\u05de\u05d4\u05dc\u05da \u05d4\u05d7\u05d5\u05d3\u05e9\u05d9\u05dd \u05d4\u05d0\u05d7\u05e8\u05d5\u05e0\u05d9\u05dd, \u05de\u05d4\u05e8\u05d2\u05e2 \u05e9\u05d4\u05d5\u05db\u05e8\u05d6\u00a0BitTorrent Bundle \u05dc\u05e7\u05d4\u05dc \u05d4\u05e8\u05d7\u05d1 \u05e2\u05d1\u05d3\u05d4 \u05d4\u05d7\u05d1\u05e8\u05d4 \u05d1\u05d9\u05d7\u05d3 \u05e2\u05dd \u05d0\u05de\u05e0\u05d9\u05dd, \u05d9\u05d5\u05e6\u05e8\u05d9\u05dd, \u05dc\u05d9\u05d9\u05d1\u05dc\u05d9\u05dd \u05d5\u05e1\u05d8\u05d5\u05d3\u05d9\u05d5 \u05d1\u05de\u05d8\u05e8\u05d4 \u05dc\u05e2\u05d6\u05d5\u05e8 \u05dc\u05d4\u05dd \u05dc\u05d1\u05e0\u05d5\u05ea \u05d0\u05ea \u05d4\u05e7\u05de\u05e4\u05d9\u05d9\u05e0\u05d9\u05dd \u05e9\u05dc\u05d4\u05dd, \u05e9\u05d9\u05d5\u05e4\u05d9\u05e2\u05d5 \u05d1\u05ea\u05d5\u05da \u05d7\u05e0\u05d5\u05ea \u05d4\u05de\u05d3\u05d9\u05d4 \u05d4\u05d7\u05d3\u05e9\u05d4. \u05db\u05e2\u05ea, \u05de\u05e9\u05d9\u05e7\u05d4 \u05d4\u05d7\u05d1\u05e8\u05d4 \u05d1\u05d0\u05dc\u05e4\u05d0 \u05e1\u05d2\u05d5\u05e8\u05d4 \u05d0\u05ea \u05d4\u05e9\u05d9\u05e8\u05d5\u05ea \u05d4\u05de\u05e9\u05d5\u05e4\u05e8, \u05d4\u05de\u05d0\u05e4\u05e9\u05e8 \u05dc\u05d0\u05de\u05e0\u05d9\u05dd \u05dc\u05d1\u05e0\u05d5\u05ea \u05d0\u05ea \u05d4\u05d7\u05e0\u05d5\u05d9\u05d5\u05ea \u05e9\u05dc\u05d4\u05dd \u05d1\u05e2\u05e6\u05de\u05dd.\n\u05d9\u05e6\u05d9\u05e8\u05ea \u05d7\u05e0\u05d5\u05ea \u05d4\u05de\u05d3\u05d9\u05d4 \u05db\u05d5\u05dc\u05dc\u05ea \u05d7\u05de\u05d9\u05e9\u05d4 \u05e6\u05e2\u05d3\u05d9\u05dd, \u05e9\u05d4\u05dd \u05d9\u05e6\u05d9\u05e8\u05ea \u05e2\u05de\u05d5\u05d3 \u05d4\u05e7\u05de\u05e4\u05d9\u05d9\u05df, \u05d4\u05d5\u05e1\u05e4\u05ea \u05d4\u05de\u05d3\u05d9\u05d4, \u05d4\u05d5\u05e1\u05e4\u05ea \u05e9\u05e2\u05e8 \u05d4\u05e0\u05e4\u05ea\u05d7 \u05d1\u05d0\u05de\u05e6\u05e2\u05d5\u05ea \u05db\u05ea\u05d5\u05d1\u05d5\u05ea \u05d4\u05de\u05d9\u05d9\u05dc \u05e9\u05dc \u05d4\u05de\u05e9\u05ea\u05de\u05e9\u05d9\u05dd, \u05e6\u05e4\u05d9\u05d9\u05d4 \u05d1\u05de\u05d5\u05e6\u05e8 \u05d4\u05de\u05d5\u05d2\u05d1\u05e8 \u05d5\u05db\u05de\u05d5\u05d1\u05df, \u05e4\u05e8\u05e1\u05d5\u05dd \u05d4\u05d1\u05d0\u05e0\u05d3\u05dc \u05d4\u05d7\u05d3\u05e9.\u00a0\u05e2\u05dd \u05d4\u05e9\u05e7\u05ea\u05d4 \u05e9\u05dc \u05d4\u05d0\u05dc\u05e4\u05d0 \u05d4\u05d7\u05d1\u05e8\u05d4 \u05db\u05d1\u05e8 \u05de\u05d5\u05e0\u05d4 \u05e9\u05de\u05d5\u05e0\u05d4 \u05e9\u05d5\u05ea\u05e4\u05d9\u05dd, \u05d4\u05db\u05d5\u05dc\u05dc\u05d9\u05dd \u05e9\u05de\u05d5\u05ea \u05de\u05d5\u05db\u05e8\u05d9\u05dd \u05d9\u05d5\u05ea\u05e8 \u05d5\u05e4\u05d7\u05d5\u05ea \u05db\u05d2\u05d5\u05df\u00a0FADER,Cinedigm,\u00a0The Collective,\u00a0Topspin,\u00a0Interloper Films,\u00a0Gravitas Ventures,Converge Studios\u00a0\u05d5-Tim Ferriss.\n\u05e9\u05d5\u05ea\u05e4\u05d4 \u05e0\u05d5\u05e1\u05e4\u05ea \u05e9\u05dc\u05de\u05e2\u05e9\u05d4 \u05d4\u05d7\u05dc\u05d4 \u05d0\u05ea \u05e9\u05d9\u05ea\u05d5\u05e3 \u05d4\u05e4\u05e2\u05d5\u05dc\u05d4 \u05e9\u05dc\u05d4 \u05e2\u05dd \u05d1\u05d9\u05d8\u05d5\u05e8\u05e0\u05d8 \u05dc\u05e4\u05e0\u05d9 \u05de\u05e1\u05e4\u05e8 \u05d7\u05d5\u05d3\u05e9\u05d9\u05dd \u05d4\u05d9\u05d0 \u05de\u05d3\u05d5\u05e0\u05d4, \u05e9\u05d7\u05d1\u05e8\u05d4 \u05dc\u05de\u05e4\u05d9\u05e7 \u05e1\u05d8\u05d9\u05d1\u05df \u05e7\u05dc\u05d9\u05d9\u05df \u05d1\u05de\u05d8\u05e8\u05d4 \u05dc\u05d9\u05e6\u05d5\u05e8 \u05e1\u05e8\u05d8 \u05e7\u05e6\u05e8 \u05d1\u05e9\u05dd\u00a0secretprojectrevolution. \u05d4\u05e1\u05e8\u05d8\u05d5\u05df \u05dc\u05de\u05e2\u05e9\u05d4 \u05de\u05e1\u05de\u05df \u05d0\u05ea \u05d4\u05d4\u05e9\u05e7\u05d4 \u05e9\u05dc \u05d4\u05e7\u05de\u05e4\u05d9\u05d9\u05df \u05d4\u05d7\u05d3\u05e9 \u05e9\u05dc\u05d4 \u2013 \u05d0\u05d5\u05de\u05e0\u05d5\u05ea \u05ea\u05de\u05d5\u05e8\u05ea \u05d7\u05d5\u05e4\u05e9 (Art for Freedom), \u05d9\u05d5\u05d6\u05de\u05d4 \u05d2\u05dc\u05d5\u05d1\u05dc\u05d9\u05ea \u05de\u05e7\u05d5\u05d5\u05e0\u05ea \u05d4\u05de\u05d9\u05d5\u05e2\u05d3\u05ea \u05dc\u05e2\u05d6\u05d5\u05e8 \u05dc\u05d4\u05e8\u05d7\u05d9\u05d1 \u05d0\u05ea \u05d7\u05d5\u05e4\u05e9 \u05d4\u05d1\u05d9\u05d8\u05d5\u05d9.\n\u05d0\u05ea \u05d4\u05e1\u05e8\u05d8\u05d5\u05df \u05ea\u05d5\u05db\u05dc\u05d5 \u05dc\u05d4\u05d5\u05e8\u05d9\u05d3 \u05d9\u05e9\u05d9\u05e8\u05d5\u05ea \u05de\u05d4\u05d0\u05ea\u05e8 \u05d4\u05e8\u05e9\u05de\u05d9 \u05e9\u05d4\u05d5\u05e7\u05dd \u05dc\u05de\u05e2\u05df \u05de\u05d8\u05e8\u05d4 \u05d6\u05d0\u05ea, \u05d4\u05e0\u05de\u05e6\u05d0 \u05d1\u05ea\u05d5\u05da \u05e7\u05d5\u05d1\u05e5\u00a0BitTorrent Bundle \u05d5\u05de\u05e6\u05d9\u05e2 \u05de\u05dc\u05d1\u05d3 \u05d4\u05e1\u05e8\u05d8\u05d5\u05df \u05e2\u05e6\u05de\u05d5 \u05d2\u05dd \u05ea\u05db\u05e0\u05d9 \u05d1\u05d5\u05e0\u05d5\u05e1 \u05db\u05d2\u05d5\u05df \u05ea\u05de\u05d5\u05e0\u05d5\u05ea \u05e1\u05d8\u05d9\u05dc\u05e1, \u05d8\u05e8\u05d9\u05d9\u05dc\u05e8\u05d9\u05dd \u05d5\u05e8\u05d0\u05d9\u05d5\u05df \u05d1\u05dc\u05e2\u05d3\u05d9 \u05e2\u05dd \u05db\u05d5\u05db\u05d1\u05ea \u05d4\u05e4\u05d5\u05e4.\n", "keywords": "\u05d1\u05d9\u05d8\u05d5\u05e8\u05e0\u05d8,\u05d1\u05d9\u05d8\u05d5\u05e8\u05e0\u05d8 \u05d1\u05d0\u05e0\u05d3\u05dc,bittorrent,bittorrent bundle,\u05de\u05d3\u05d5\u05e0\u05d4,madonna,\u05d4\u05d5\u05e8\u05d3\u05d4 \u05d9\u05e9\u05d9\u05e8\u05d4,\u05d1\u05d9\u05d8\u05d8\u05d5\u05e8\u05e0\u05d8,\u05d4\u05d5\u05e8\u05d3\u05d5\u05ea,"}