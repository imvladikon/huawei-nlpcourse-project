5e9\u05dc \u05db\u05d5\u05e8\u05d9 \u05d1\u05d9\u05d8\u05e7\u05d5\u05d9\u05df \u05d5\u05de\u05d8\u05d1\u05e2\u05d5\u05ea \u05e7\u05e8\u05d9\u05e4\u05d8\u05d5\u05d2\u05e8\u05e4\u05d9\u05d9\u05dd \u05d0\u05d7\u05e8\u05d9\u05dd. \u05d4\u05d4\u05e9\u05dc\u05db\u05d5\u05ea \u05de\u05d8\u05e8\u05d9\u05d3\u05d5\u05ea \u05d1\u05de\u05d9\u05d5\u05d7\u05d3\n\u05de\u05e7\u05d5\u05e8: Pixabay\n\u05d1\u05e9\u05e0\u05d4 \u05d4\u05d7\u05d5\u05dc\u05e4\u05ea \u05e8\u05d0\u05d9\u05e0\u05d5 \u05db\u05d9\u05e6\u05d3 \u05d4\u05de\u05d8\u05d1\u05e2\u05d5\u05ea \u05d4\u05e7\u05e8\u05d9\u05e4\u05d8\u05d5\u05d2\u05e8\u05e4\u05d9\u05d9\u05dd \u05e4\u05d5\u05e8\u05e6\u05d9\u05dd \u05e2\u05dc \u05d4\u05ea\u05d5\u05d3\u05e2\u05d4 \u05d5\u05de\u05d2\u05d9\u05e2\u05d9\u05dd \u05d0\u05dc \u05d4\u05e7\u05d4\u05dc \u05d4\u05e8\u05d7\u05d1. \u05db\u05de\u05d5\u05d1\u05df \u05e9\u201d\u05d4\u05d0\u05e9\u05dd\u201d \u05d4\u05e2\u05d9\u05e7\u05e8\u05d9 \u05d4\u05d5\u05d0 \u05e9\u05e2\u05e8\u05d5 \u05e9\u05dc \u05d4\u05d1\u05d9\u05d8\u05e7\u05d5\u05d9\u05df, \u05d4\u05de\u05d8\u05d1\u05e2 \u05d4\u05e7\u05e8\u05d9\u05e4\u05d8\u05d5\u05d2\u05e8\u05e4\u05d9 \u05d4\u05e8\u05d0\u05e9\u05d5\u05df \u05d5\u05d4\u05e4\u05d5\u05e4\u05d5\u05dc\u05e8\u05d9 \u05d1\u05d9\u05d5\u05ea\u05e8 \u05d1\u05e2\u05d5\u05dc\u05dd, \u05e9\u05e9\u05d1\u05e8 \u05e9\u05d9\u05d0 \u05d0\u05d7\u05e8\u05d9 \u05e9\u05d9\u05d0. \u05d1\u05de\u05e7\u05d1\u05d9\u05dc \u05e8\u05d0\u05d9\u05e0\u05d5 \u05d2\u05dd \u05e4\u05e8\u05d9\u05d7\u05d4 \u05d1-ICO, \u05d4\u05e0\u05e4\u05e7\u05d5\u05ea \u05d4\u05de\u05d8\u05d1\u05e2 \u05d4\u05e8\u05d0\u05e9\u05d5\u05e0\u05d9\u05d5\u05ea \u05e9\u05d0\u05e4\u05e9\u05e8\u05d5 \u05dc\u05e1\u05d8\u05d0\u05e8\u05d8\u05d0\u05e4\u05d9\u05dd \u2013 \u05d5\u05db\u05df, \u05d2\u05dd \u05dc\u05dc\u05d0 \u05de\u05e2\u05d8 \u05e8\u05de\u05d0\u05d9\u05dd \u2013 \u05dc\u05d2\u05d9\u05d9\u05e1 \u05db\u05e1\u05e4\u05d9\u05dd \u05de\u05d4\u05e6\u05d9\u05d1\u05d5\u05e8 \u05d1\u05e2\u05d6\u05e8\u05ea \u05d8\u05d5\u05e7\u05e0\u05d9\u05dd \u05d5\u05de\u05d8\u05d1\u05e2\u05d5\u05ea \u05e7\u05e8\u05d9\u05e4\u05d8\u05d5\u05d2\u05e8\u05e4\u05d9\u05d9\u05dd \u05d7\u05d3\u05e9\u05d9\u05dd. \u05d4\u05e2\u05dc\u05d9\u05d9\u05d4 \u05d1\u05de\u05d5\u05d3\u05e2\u05d5\u05ea \u05d5\u05d1\u05d1\u05d9\u05e7\u05d5\u05e9 \u05dc\u05de\u05d8\u05d1\u05e2\u05d5\u05ea \u05db\u05de\u05d5 \u05d1\u05d9\u05d8\u05e7\u05d5\u05d9\u05df \u05d5\u05dc\u05d9\u05d9\u05d8\u05e7\u05d5\u05d9\u05df \u05d9\u05e6\u05e8\u05d5 \u05d3\u05e8\u05d9\u05e9\u05d4 \u05d7\u05e1\u05e8\u05ea \u05ea\u05e7\u05d3\u05d9\u05dd \u05d2\u05dd \u05dc\u05db\u05e8\u05d9\u05d9\u05d4 \u05e9\u05dc \u05d4\u05de\u05d8\u05d1\u05e2\u05d5\u05ea. \u05d0\u05d5\u05dc\u05dd \u05d1\u05e6\u05d3 \u05d4\u05d9\u05ea\u05e8\u05d5\u05e0\u05d5\u05ea \u05e9\u05dc \u05d4\u05de\u05d8\u05d1\u05e2\u05d5\u05ea \u05d4\u05e7\u05e8\u05d9\u05e4\u05d8\u05d5\u05d2\u05e8\u05e4\u05d9\u05d9\u05dd, \u05ea\u05d4\u05dc\u05d9\u05da \u05d4\u05db\u05e8\u05d9\u05d9\u05d4 \u05e9\u05dc\u05d4\u05dd \u05d3\u05d5\u05e8\u05e9 \u05de\u05e9\u05d0\u05d1\u05d9 \u05de\u05d9\u05d7\u05e9\u05d5\u05d1 \u05e2\u05e6\u05d5\u05de\u05d9\u05dd \u05e9\u05d2\u05d5\u05e8\u05de\u05d9\u05dd \u05dc\u05dc\u05d0 \u05de\u05e2\u05d8 \u05ea\u05d5\u05e4\u05e2\u05d5\u05ea \u05dc\u05d5\u05d5\u05d0\u05d9 \u05e9\u05dc\u05d9\u05dc\u05d9\u05d5\u05ea.\n\u05db\u05e8\u05d8\u05d9\u05e1\u05d9\u05dd \u05d2\u05e8\u05e4\u05d9\u05d9\u05dd, \u05d5\u05d1\u05e2\u05d9\u05e7\u05e8 \u05d4\u05de\u05e2\u05d1\u05d3 \u05e9\u05dc\u05d4\u05dd \u2013 GPU \u2013 \u05d4\u05d5\u05d0 \u05d0\u05d7\u05d3 \u05d4\u05e8\u05db\u05d9\u05d1\u05d9\u05dd \u05d4\u05d7\u05e9\u05d5\u05d1\u05d9\u05dd \u05d1\u05d9\u05d5\u05ea\u05e8 \u05dc\u05e6\u05d5\u05e8\u05da \u05db\u05e8\u05d9\u05d9\u05ea \u05de\u05d8\u05d1\u05e2\u05d5\u05ea \u05e7\u05e8\u05d9\u05e4\u05d8\u05d5\u05d2\u05e8\u05e4\u05d9\u05d9\u05dd, \u05d5\u05de\u05e2\u05d1\u05d3 \u05d2\u05e8\u05e4\u05d9 \u05d7\u05d6\u05e7 \u05d9\u05db\u05d5\u05dc \u05dc\u05db\u05e8\u05d5\u05ea \u05de\u05d8\u05d1\u05e2\u05d5\u05ea \u05e7\u05e8\u05d9\u05e4\u05d8\u05d5\u05d2\u05e8\u05e4\u05d9\u05d9\u05dd \u05d1\u05e9\u05d5\u05d5\u05d9 \u05db\u05de\u05d4 \u05d3\u05d5\u05dc\u05e8\u05d9\u05dd \u05d1\u05d9\u05d5\u05dd. \u05d4\u05ea\u05d5\u05e6\u05d0\u05d4: \u05db\u05dc \u05de\u05e9\u05ea\u05de\u05e9 \u05e9\u05e8\u05d5\u05e6\u05d4 \u05dc\u05d1\u05e0\u05d5\u05ea \u05dc\u05e2\u05e6\u05de\u05d5 \u05de\u05d7\u05e9\u05d1 \u05db\u05e8\u05d9\u05d9\u05d4 \u05e6\u05e8\u05d9\u05da \u05dc\u05e8\u05db\u05d5\u05e9 GPU \u05de\u05ea\u05e7\u05d3\u05dd. \u05e2\u05db\u05e9\u05d9\u05d5 \u05ea\u05db\u05e4\u05d9\u05dc\u05d5 \u05d0\u05ea \u05d6\u05d4 \u05d1\u05de\u05d0\u05d5\u05ea \u05d0\u05dc\u05e4\u05d9 \u05d4\u05de\u05e2\u05d1\u05d3\u05d9\u05dd \u05d4\u05de\u05ea\u05e7\u05d3\u05de\u05d9\u05dd \u05e9\u05e6\u05e8\u05d9\u05db\u05d5\u05ea \u05dc\u05e8\u05db\u05d5\u05e9 \u05d7\u05d1\u05e8\u05d5\u05ea \u05d4\u05db\u05e8\u05d9\u05d9\u05d4 \u05d4\u05d2\u05d3\u05d5\u05dc\u05d5\u05ea \u05d5\u05ea\u05e7\u05d1\u05dc\u05d5\u00a0\u05de\u05d7\u05e1\u05d5\u05e8, \u05e9\u05d1\u05e2\u05e7\u05d1\u05d5\u05ea\u05d9\u05d5 Resellers \u05de\u05e2\u05dc\u05d9\u05dd \u05d0\u05ea \u05d4\u05de\u05d7\u05d9\u05e8\u05d9\u05dd \u05d1\u05e6\u05d5\u05e8\u05d4 \u05d3\u05e8\u05e1\u05d8\u05d9\u05ea, \u05d5\u05dc\u05d0 \u05de\u05e9\u05d0\u05d9\u05e8\u05d9\u05dd \u05db\u05e8\u05d8\u05d9\u05e1\u05d9 \u05de\u05e1\u05da \u05d1\u05de\u05d7\u05d9\u05e8\u05d9\u05dd \u05d4\u05d5\u05d2\u05e0\u05d9\u05dd \u05dc\u05d2\u05d9\u05d9\u05de\u05e8\u05d9\u05dd \u05d0\u05d5 \u05de\u05e9\u05ea\u05de\u05e9\u05d9\u05dd \u05e1\u05d8\u05e0\u05d3\u05e8\u05d8\u05d9\u05dd.\n\u05d0\u05de\u05e0\u05dd \u05d4\u05db\u05e8\u05d9\u05d9\u05d4 \u05e9\u05dc \u05d1\u05d9\u05d8\u05e7\u05d5\u05d9\u05df \u05e2\u05dc \u05d9\u05d3\u05d9 \u05de\u05e9\u05ea\u05de\u05e9\u05d9\u05dd \u05d1\u05d9\u05ea\u05d9\u05d9\u05dd \u05db\u05de\u05e2\u05d8 \u05dc\u05d0 \u05de\u05e9\u05ea\u05dc\u05de\u05ea, \u05d0\u05da \u05db\u05e8\u05d9\u05d9\u05d4 \u05e9\u05dc \u05de\u05d8\u05d1\u05e2\u05d5\u05ea \u05e7\u05e6\u05ea \u05e4\u05d7\u05d5\u05ea \u05e4\u05d5\u05e4\u05d5\u05dc\u05e8\u05d9\u05d9\u05dd \u05db\u05de\u05d5 \u05d0\u05d9\u05ea\u2019\u05e8\u05d9\u05d5\u05dd \u05d1\u05d0\u05de\u05e6\u05e2\u05d5\u05ea \u05db\u05e8\u05d8\u05d9\u05e1\u05d9 \u05de\u05e1\u05da \u05d1\u05e2\u05dc\u05d9 \u05d6\u05d9\u05db\u05e8\u05d5\u05df \u05d2\u05d3\u05d5\u05dc \u05d9\u05d5\u05ea\u05e8 \u05de-2 \u05d2\u05d9\u05d2\u05d4, \u05d9\u05db\u05d5\u05dc\u05d4 \u05dc\u05d4\u05e0\u05d9\u05d1 \u05e1\u05db\u05d5\u05de\u05d9\u05dd \u05e0\u05d0\u05d9\u05dd \u05dc\u05de\u05e9\u05ea\u05de\u05e9\u05d9\u05dd. \u05db\u05d5\u05e8\u05d4 \u05de\u05de\u05d5\u05e6\u05e2 \u05de\u05e8\u05d5\u05d5\u05d9\u05d7 \u05d1\u05e2\u05e8\u05da 50 \u05d3\u05d5\u05dc\u05e8 \u05dc\u05d9\u05d5\u05dd \u05d1\u05e2\u05d6\u05e8\u05ea 14 \u05db\u05e8\u05d8\u05d9\u05e1\u05d9\u05dd \u05d2\u05e8\u05e4\u05d9\u05d9\u05dd \u05de\u05ea\u05e7\u05d3\u05de\u05d9\u05dd, \u05db\u05dc\u05d5\u05de\u05e8 \u05d1\u05e2\u05e8\u05da 3.5 \u05d3\u05d5\u05dc\u05e8 \u05dc\u05db\u05e8\u05d8\u05d9\u05e1 \u05d1\u05db\u05dc \u05d9\u05d5\u05dd, \u05d5\u05d1\u05e7\u05e6\u05d1 \u05d6\u05d4 \u05d4\u05db\u05e8\u05d8\u05d9\u05e1 \u05d9\u05d4\u05e4\u05d5\u05da \u05dc\u05de\u05e0\u05d9\u05d1 \u05d1\u05ea\u05d5\u05da \u05d0\u05e8\u05d1\u05e2\u05d4 \u05d7\u05d5\u05d3\u05e9\u05d9\u05dd.\n\u05d0\u05d1\u05dc \u05dc\u05de\u05d4 \u05dc\u05e9\u05dc\u05dd \u05d4\u05e8\u05d1\u05d4 \u05db\u05e1\u05e3 \u05e2\u05dc \u05de\u05e2\u05d1\u05d3\u05d9\u05dd \u05d2\u05e8\u05e4\u05d9\u05d9\u05dd \u05d5\u05d7\u05e9\u05de\u05dc \u05d9\u05e7\u05e8, \u05d0\u05dd \u05d0\u05e4\u05e9\u05e8 \u05e4\u05e9\u05d5\u05d8 \u05dc\u05e0\u05e6\u05dc \u05d0\u05ea \u05de\u05e9\u05d0\u05d1\u05d9 \u05d4\u05de\u05d9\u05d7\u05e9\u05d5\u05d1 \u05e9\u05dc \u05d0\u05e0\u05e9\u05d9\u05dd \u05ea\u05de\u05d9\u05de\u05d9\u05dd? 2017 \u05d4\u05d9\u05ea\u05d4 \u05e9\u05e0\u05ea \u05e9\u05d9\u05d0 \u05dc\u05e0\u05d5\u05d6\u05e7\u05d5\u05ea \u05de\u05e1\u05d5\u05d2\u00a0Cryptojacking. \u05d1\u05e7\u05e6\u05e8\u05d4, \u05de\u05d3\u05d5\u05d1\u05e8 \u05e2\u05dc \u05e7\u05d8\u05e2 \u05e7\u05d5\u05d3 \u05e9\u05e8\u05e5 \u2013 \u05d1\u05d3\u05e8\u05da \u05db\u05dc\u05dc \u05d1\u05d3\u05e4\u05d3\u05e4\u05df \u05e9\u05dc\u05db\u05dd \u2013 \u05d5\u05e8\u05d5\u05ea\u05dd \u05d0\u05ea \u05db\u05dc \u05de\u05e9\u05d0\u05d1\u05d9 \u05d4\u05de\u05d7\u05e9\u05d1 \u05e9\u05dc\u05db\u05dd \u05dc\u05e6\u05d5\u05e8\u05da \u05db\u05e8\u05d9\u05d9\u05d4 \u05e9\u05dc \u05de\u05d8\u05d1\u05e2\u05d5\u05ea, \u05db\u05e9\u05d4\u05ea\u05de\u05d5\u05e8\u05d4 \u05db\u05de\u05d5\u05d1\u05df \u05de\u05d5\u05e2\u05d1\u05e8\u05ea \u05dc\u05d0\u05e8\u05e0\u05e7 \u05e9\u05dc \u05d4\u05ea\u05d5\u05e7\u05e3. \u05d1\u05d9\u05df \u05d4\u05d9\u05ea\u05e8 \u05e8\u05d0\u05d9\u05e0\u05d5 \u05e0\u05d5\u05d6\u05e7\u05d5\u05ea \u05db\u05e8\u05d9\u05d9\u05d4 \u05e9\u05de\u05d3\u05d1\u05d9\u05e7\u05d5\u05ea \u05de\u05d7\u05e9\u05d1\u05d9\u05dd\u00a0\u05d1\u05ea\u05d5\u05da \u05d0\u05e4\u05dc\u05d9\u05e7\u05e6\u05d9\u05d5\u05ea \u05e4\u05d5\u05e4\u05d5\u05dc\u05e8\u05d9\u05d5\u05ea \u05dc\u05de\u05d7\u05e9\u05d1, \u05d3\u05e8\u05da \u05d4\u05de\u05e1\u05e0\u05d2\u2019\u05e8 \u05e9\u05dc \u05e4\u05d9\u05d9\u05e1\u05d1\u05d5\u05e7, \u05d3\u05e8\u05da \u05e6\u05e4\u05d9\u05d9\u05d4 \u05d1\u05d9\u05d5\u05d8\u05d9\u05d5\u05d1; \u05d5\u05d0\u05e4\u05d9\u05dc\u05d5 \u05d0\u05ea\u05e8\u05d9\u05dd \u05d9\u05e9\u05e8\u05d0\u05dc\u05d9\u05d9\u05dd \u05db\u05de\u05d5 \u05de\u05d9\u05dc\u05d5\u05df \u05de\u05d5\u05e8\u05e4\u05d9\u05e7\u05e1 \u05d4\u05e4\u05d5\u05e4\u05d5\u05dc\u05e8\u05d9 \u05d5\u05d4-Jerusalem Post \u05e9\u05e0\u05e4\u05dc\u05d5 \u05e7\u05d5\u05e8\u05d1\u05df \u05dc\u05e0\u05d5\u05d6\u05e7\u05d5\u05ea \u05db\u05d0\u05dc\u05d5 \u2013 \u05dc\u05e8\u05d5\u05d1 \u05d1\u05d0\u05e9\u05de\u05ea \u05de\u05d5\u05d3\u05e2\u05d5\u05ea \u05d4\u05e4\u05e8\u05e1\u05d5\u05dd \u05d1\u05d4\u05df. \u05d1\u05de\u05e7\u05e8\u05d9\u05dd \u05e7\u05d9\u05e6\u05d5\u05e0\u05d9\u05d9\u05dd \u05d4\u05e0\u05d5\u05d6\u05e7\u05d5\u05ea \u05e2\u05dc\u05d5\u05dc\u05d5\u05ea \u05d0\u05e4\u05d9\u05dc\u05d5 \u05dc\u05d4\u05e9\u05d1\u05d9\u05ea \u05de\u05db\u05e9\u05d9\u05e8\u05d9\u05dd.\n\u05d1\u05ea\u05d2\u05d5\u05d1\u05d4 \u05d4\u05d5\u05e4\u05d9\u05e2\u05d5 \u05db\u05dc\u05d9\u05dd \u05e9\u05d5\u05e0\u05d9\u05dd \u05e9\u05e0\u05d5\u05e2\u05d3\u05d5 \u05dc\u05d4\u05d2\u05df \u05e2\u05dc \u05d4\u05de\u05e9\u05ea\u05de\u05e9\u05d9\u05dd. \u05d1\u05d9\u05df \u05d4\u05d9\u05ea\u05e8 \u05d3\u05e4\u05d3\u05e4\u05df Opera \u05e9\u05d4\u05e6\u05d9\u05d2 \u05d7\u05d5\u05e1\u05dd \u05db\u05e8\u05d9\u05d9\u05ea \u05e7\u05e8\u05d9\u05e4\u05d8\u05d5 \u05de\u05d5\u05d1\u05e0\u05d4\u00a0\u05d5\u05d7\u05d5\u05e1\u05dd \u05d4\u05e4\u05e8\u05e1\u05d5\u05de\u05d5\u05ea \u05d4\u05e4\u05d5\u05e4\u05d5\u05dc\u05e8\u05d9 \u05d1\u05d9\u05d5\u05ea\u05e8 \u05d1\u05e2\u05d5\u05dc\u05dd, \u05e9\u05d4\u05d5\u05e1\u05d9\u05e3 \u05d7\u05e1\u05d9\u05de\u05d4 \u05e9\u05dc \u05e1\u05e7\u05e8\u05d9\u05e4\u05d8\u05d9\u05dd \u05dc\u05db\u05e8\u05d9\u05d9\u05ea \u05e7\u05e8\u05d9\u05e4\u05d8\u05d5.\n\u05de\u05ea\u05d1\u05e8\u05e8 \u05e9\u05dc\u05d0 \u05e8\u05e7 \u05e9\u05e2\u05e8 \u05d4\u05d1\u05d9\u05d8\u05e7\u05d5\u05d9\u05df \u05e9\u05d1\u05e8 \u05e9\u05d9\u05d0\u05d9\u05dd \u05d1-2017, \u05d0\u05dc\u05d0 \u05d2\u05dd \u05e6\u05e8\u05d9\u05db\u05ea \u05d4\u05d7\u05e9\u05de\u05dc \u05e9\u05e0\u05d5\u05d1\u05e2\u05ea \u05de\u05ea\u05d4\u05dc\u05d9\u05da \u05d4\u05db\u05e8\u05d9\u05d9\u05d4 \u05e9\u05dc\u05d5. \u05d6\u05d5\u05db\u05e8\u05d9\u05dd \u05d0\u05ea \u05d4\u05d7\u05d9\u05e9\u05d5\u05d1\u05d9\u05dd \u05d4\u05de\u05ea\u05de\u05d8\u05d9\u05d9\u05dd \u05d5\u05d4\u05e7\u05e8\u05d9\u05e4\u05d8\u05d5\u05d2\u05e8\u05e4\u05d9\u05d9\u05dd \u05d4\u05de\u05d5\u05e8\u05db\u05d1\u05d9\u05dd \u05e9\u05e6\u05e8\u05d9\u05db\u05d9\u05dd \u05dc\u05e2\u05e9\u05d5\u05ea \u05d4\u05de\u05d7\u05e9\u05d1\u05d9\u05dd \u05dc\u05e6\u05d5\u05e8\u05da \u05db\u05e8\u05d9\u05d9\u05ea \u05d4\u05de\u05d8\u05d1\u05e2\u05d5\u05ea? \u05de\u05db\u05d9\u05d5\u05d5\u05df \u05e9\u05d4\u05ea\u05de\u05d5\u05e8\u05d4 \u05de\u05d5\u05e2\u05d1\u05e8\u05ea \u05dc\u05de\u05d7\u05e9\u05d1 \u05e9\u05d1\u05d9\u05e6\u05e2 \u05d0\u05ea \u05d4\u05d7\u05d9\u05e9\u05d5\u05d1\u05d9\u05dd \u05d4\u05dc\u05dc\u05d5 \u05d1\u05d6\u05de\u05df \u05d4\u05e7\u05e6\u05e8 \u05d1\u05d9\u05d5\u05ea\u05e8, \u05e0\u05d5\u05e6\u05e8\u00a0\u05de\u05e2\u05d9\u05df \u05de\u05e8\u05d5\u05e5 \u05d7\u05d9\u05de\u05d5\u05e9 \u05e9\u05dc \u05db\u05d5\u05e8\u05d9\u05dd, \u05e9\u05de\u05e0\u05e1\u05d9\u05dd \u05dc\u05d4\u05e6\u05d8\u05d9\u05d9\u05d3 \u05d1\u05de\u05d7\u05e9\u05d1\u05d9\u05dd \u05d4\u05d7\u05d6\u05e7\u05d9\u05dd \u05d1\u05d9\u05d5\u05ea\u05e8 \u05d5\u05dc\u05db\u05df \u05d2\u05dd \u05d4\u05d6\u05dc\u05dc\u05e0\u05d9\u05d9\u05dd \u05d1\u05d9\u05d5\u05ea\u05e8 \u05d1\u05d7\u05e9\u05de\u05dc.\u00a0\u05de\u05de\u05d7\u05e7\u05e8 \u05e9\u05d1\u05d9\u05e6\u05e2\u05d4\u00a0Power Compare,\u00a0\u05d7\u05d1\u05e8\u05ea \u05d4\u05e9\u05d5\u05d5\u05d0\u05ea \u05de\u05d7\u05d9\u05e8\u05d9 \u05d4\u05d0\u05e0\u05e8\u05d2\u05d9\u05d4 \u05d4\u05d1\u05e8\u05d9\u05d8\u05d9\u05ea, \u05e2\u05d5\u05dc\u05d4 \u05e9\u05d4\u05e9\u05e0\u05d4 \u05d4\u05d2\u05d9\u05e2\u05d4 \u05e6\u05e8\u05d9\u05db\u05ea \u05d4\u05d7\u05e9\u05de\u05dc \u05d4\u05e9\u05e0\u05ea\u05d9\u05ea \u05e9\u05dc \u05de\u05d7\u05e9\u05d1\u05d9 \u05db\u05e8\u05d9\u05d9\u05ea \u05d4\u05d1\u05d9\u05d8\u05e7\u05d5\u05d9\u05df \u05dc\u05e9\u05d9\u05d0 \u05e9\u05dc 29.05 \u05d8\u05e8\u05d4\u05d5\u05d0\u05d8\u05be\u05e9\u05e2\u05d4 (TWh), \u05d4\u05de\u05e7\u05d1\u05d9\u05dc\u05d4 \u05dc-0.13 \u05d0\u05d7\u05d5\u05d6 \u05de\u05e6\u05e8\u05d9\u05db\u05ea \u05d4\u05d7\u05e9\u05de\u05dc \u05d4\u05e2\u05d5\u05dc\u05de\u05d9\u05ea. \u05dc\u05e9\u05dd \u05d4\u05e9\u05d5\u05d5\u05d0\u05d4, \u05d4\u05e6\u05e8\u05d9\u05db\u05d4 \u05d4\u05d6\u05d5 \u05d2\u05d3\u05d5\u05dc\u05d4 \u05d9\u05d5\u05ea\u05e8 \u05de\u05e6\u05e8\u05d9\u05db\u05ea \u05d4\u05d7\u05e9\u05de\u05dc \u05e9\u05dc 159 \u05de\u05d3\u05d9\u05e0\u05d5\u05ea \u05e9\u05d5\u05e0\u05d5\u05ea, \u05d1\u05d4\u05df \u05d0\u05d9\u05e8\u05dc\u05e0\u05d3, \u05e0\u05d9\u05d2\u05e8\u05d9\u05d4, \u05d4\u05d5\u05e0\u05d2\u05e8\u05d9\u05d4, \u05e7\u05e8\u05d5\u05d0\u05d8\u05d9\u05d4 \u05d0\u05d5 \u05d0\u05d5\u05e8\u05d2\u05d5\u05d5\u05d0\u05d9. \u05d1\u05d4\u05e9\u05d5\u05d5\u05d0\u05d4 \u05dc\u05d9\u05e9\u05e8\u05d0\u05dc, \u05d0\u05d2\u05d1, \u05de\u05d3\u05d5\u05d1\u05e8 \u05e2\u05dc 48.55 \u05d0\u05d7\u05d5\u05d6 \u05d0\u05d5 \u05db\u05de\u05e2\u05d8 \u05de\u05d7\u05e6\u05d9\u05ea \u05de\u05e6\u05e8\u05d9\u05db\u05ea \u05d4\u05d7\u05e9\u05de\u05dc \u05d4\u05e9\u05e0\u05ea\u05d9\u05ea \u05e9\u05dc\u05e0\u05d5.\n\u05d0\u05d5\u05dc\u05dd, \u05d0\u05dd \u05e0\u05e9\u05d9\u05dd \u05e8\u05d2\u05e2 \u05d1\u05e6\u05d3 \u05d0\u05ea \u05e0\u05ea\u05d5\u05e0\u05d9 \u05e6\u05e8\u05d9\u05db\u05ea \u05d4\u05d7\u05e9\u05de\u05dc \u05e9\u05dc \u05de\u05d7\u05e9\u05d1\u05d9 \u05d4\u05db\u05e8\u05d9\u05d9\u05d4 \u05db\u05d9\u05d5\u05dd, \u05d4\u05e8\u05d9 \u05e9\u05de\u05d4 \u05e9\u05de\u05d8\u05e8\u05d9\u05d3 \u05d1\u05d0\u05de\u05ea \u05d4\u05d5\u05d0 \u05e7\u05e6\u05d1 \u05d4\u05d2\u05d9\u05d3\u05d5\u05dc \u05d1\u05e6\u05e8\u05d9\u05db\u05d4. \u05e2\u05dc \u05e4\u05d9 \u05e0\u05ea\u05d5\u05e0\u05d9 \u05d4\u05de\u05d7\u05e7\u05e8, \u05d0\u05dd \u05d9\u05d9\u05de\u05e9\u05da \u05e7\u05e6\u05d1 \u05d4\u05d2\u05d9\u05d3\u05d5\u05dc \u05d4\u05e0\u05d5\u05db\u05d7\u05d9, \u05ea\u05e2\u05d1\u05d5\u05e8 \u05e6\u05e8\u05d9\u05db\u05ea \u05d4\u05d7\u05e9\u05de\u05dc \u05e9\u05dc \u05ea\u05e2\u05e9\u05d9\u05d9\u05ea \u05db\u05e8\u05d9\u05d9\u05ea \u05d4\u05d1\u05d9\u05d8\u05e7\u05d5\u05d9\u05df \u05d4\u05e2\u05d5\u05dc\u05de\u05d9\u05ea \u05d0\u05ea \u05e6\u05e8\u05d9\u05db\u05ea \u05d4\u05d7\u05e9\u05de\u05dc \u05d4\u05e9\u05e0\u05ea\u05d9\u05ea \u05e9\u05dc \u05d1\u05e8\u05d9\u05d8\u05e0\u05d9\u05d4 \u05ea\u05d5\u05da \u05e4\u05d7\u05d5\u05ea \u05de\u05e9\u05e0\u05d4, \u05d5\u05ea\u05d4\u05d9\u05d4 \u05e8\u05d5\u05d1 \u05de\u05db\u05e8\u05d9\u05e2 \u05e9\u05dc \u05e6\u05e8\u05d9\u05db\u05ea \u05d4\u05d7\u05e9\u05de\u05dc \u05e9\u05dc \u05d4\u05e2\u05d5\u05dc\u05dd \u05e2\u05d3 \u05e4\u05d1\u05e8\u05d5\u05d0\u05e8 2020.\n\u00a0", "keywords": "\u05d1\u05d9\u05d8\u05e7\u05d5\u05d9\u05df,\u05db\u05e8\u05d9\u05d9\u05d4,\u05d0\u05dc\u05d8\u05e7\u05d5\u05d9\u05df,\u05e7\u05e8\u05d9\u05e4\u05d8\u05d5,\u05e7\u05e8\u05d9\u05e4\u05d8\u05d5\u05d2\u05e8\u05e4\u05d9,\u05de\u05d8\u05d1\u05e2 \u05e7\u05e8\u05d9\u05e4\u05d8\u05d5\u05d2\u05e8\u05e4\u05d9,\u05de\u05d8\u05d1\u05e2 \u05d3\u05d9\u05d2\u05d9\u05d8\u05dc\u05d9,\u05de\u05d5\u05e0\u05e8\u05d5,\u05dc\u05d9\u05d9\u05d8\u05e7\u05d5\u05d9\u05df"}