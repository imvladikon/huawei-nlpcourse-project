05d5\u05d7 \u05e0\u05d3\u05d9\u05e8.     \u05de\u05e8\u05db\u05d9\u05d1\u05d9\u05dd  \u05ea\u05d1\u05e0\u05d9\u05ea \u05d1\u05e7\u05d5\u05d8\u05e8 24 \u05e1\"\u05de \u05d0\u05d5 \u05d1\u05d2\u05d5\u05d3\u05dc 20X20 \u05e1\"\u05de   \u05dc\u05e7\u05e8\u05dd \u05d4\u05dc\u05d9\u05de\u05d5\u05df  1 \u05d1\u05d9\u05e6\u05d4  2 \u05d7\u05dc\u05de\u05d5\u05e0\u05d9\u05dd  100 \u05d2\u05e8\u05dd (1/2 \u05db\u05d5\u05e1) \u05e1\u05d5\u05db\u05e8  120 \u05de\"\u05dc (1/2 \u05db\u05d5\u05e1) \u05de\u05d9\u05e5 \u05dc\u05d9\u05de\u05d5\u05df  1 \u05db\u05e3 \u05e7\u05d5\u05e8\u05e0\u05e4\u05dc\u05d5\u05e8   \u05dc\u05d1\u05e8\u05d0\u05d5\u05e0\u05d9\u05d6  150 \u05d2\u05e8\u05dd \u05d7\u05de\u05d0\u05d4  150 \u05d2\u05e8\u05dd \u05e9\u05d5\u05e7\u05d5\u05dc\u05d3 \u05de\u05e8\u05d9\u05e8  2 \u05d1\u05d9\u05e6\u05d9\u05dd  200 \u05d2\u05e8\u05dd (1 \u05db\u05d5\u05e1) \u05e1\u05d5\u05db\u05e8  35 \u05d2\u05e8\u05dd (1/4 \u05db\u05d5\u05e1) \u05d0\u05d1\u05e7\u05ea \u05e7\u05e7\u05d0\u05d5  70 \u05d2\u05e8\u05dd (1/2 \u05db\u05d5\u05e1) \u05e7\u05de\u05d7  1/4 \u05db\u05e4\u05d9\u05ea \u05de\u05dc\u05d7   \u05d0\u05e4\u05e9\u05e8\u05d5\u05ea: 20 \u05d2\u05e8\u05dd \u05e9\u05d5\u05e7\u05d5\u05dc\u05d3 \u05de\u05e8\u05d9\u05e8 \u05de\u05d5\u05de\u05e1 \u05dc\u05e7\u05d9\u05e9\u05d5\u05d8       \u05d0\u05d5\u05e4\u05df \u05d4\u05d4\u05db\u05e0\u05d4  \u05e7\u05e8\u05dd \u05d4\u05dc\u05d9\u05de\u05d5\u05df: \u05e9\u05de\u05d9\u05dd \u05d0\u05ea \u05db\u05dc \u05d4\u05de\u05e8\u05db\u05d9\u05d1\u05d9\u05dd \u05d1\u05e1\u05d9\u05e8 \u05e7\u05d8\u05df \u05d5\u05de\u05d1\u05e9\u05dc\u05d9\u05dd \u05e2\u05dc \u05d0\u05e9 \u05d1\u05d9\u05e0\u05d5\u05e0\u05d9\u05ea, \u05ea\u05d5\u05da \u05e2\u05e8\u05d1\u05d5\u05d1, \u05e2\u05d3 \u05e9\u05d4\u05e7\u05e8\u05dd \u05de\u05ea\u05d7\u05d9\u05dc \u05dc\u05d4\u05e1\u05de\u05d9\u05da. \u05e9\u05d5\u05de\u05e8\u05d9\u05dd \u05d1\u05e6\u05d3.  \u05d4\u05d1\u05e8\u05d0\u05d5\u05e0\u05d9\u05d6: \u05de\u05d7\u05de\u05de\u05d9\u05dd \u05d0\u05ea \u05d4\u05ea\u05e0\u05d5\u05e8 \u05dc-175 \u05de\u05e2\u05dc\u05d5\u05ea. \u05de\u05e8\u05e4\u05d3\u05d9\u05dd \u05d0\u05ea \u05d4\u05ea\u05d1\u05e0\u05d9\u05ea \u05d1\u05e0\u05d9\u05d9\u05e8 \u05d0\u05e4\u05d9\u05d9\u05d4.  \u05de\u05de\u05d9\u05e1\u05d9\u05dd \u05d9\u05d7\u05d3 \u05d0\u05ea \u05d4\u05d7\u05de\u05d0\u05d4 \u05d5\u05d4\u05e9\u05d5\u05e7\u05d5\u05dc\u05d3 \u05e2\u05dc \u05d1\u05d0\u05df-\u05de\u05e8\u05d9 \u05d0\u05d5 \u05d1\u05de\u05d9\u05e7\u05e8\u05d5\u05d2\u05dc, \u05e2\u05d3 \u05dc\u05e7\u05d1\u05dc\u05ea \u05ea\u05e2\u05e8\u05d5\u05d1\u05ea \u05d7\u05dc\u05e7\u05d4.  \u05de\u05d5\u05e1\u05d9\u05e4\u05d9\u05dd \u05d0\u05ea \u05d4\u05d1\u05d9\u05e6\u05d9\u05dd \u05d5\u05d4\u05e1\u05d5\u05db\u05e8 \u05d5\u05d8\u05d5\u05e8\u05e4\u05d9\u05dd \u05d4\u05d9\u05d8\u05d1.  \u05de\u05d5\u05e1\u05d9\u05e4\u05d9\u05dd \u05d0\u05ea \u05d4\u05e7\u05e7\u05d0\u05d5, \u05d4\u05e7\u05de\u05d7 \u05d5\u05d4\u05de\u05dc\u05d7 \u05d5\u05de\u05e7\u05e4\u05dc\u05d9\u05dd \u05d1\u05e2\u05d6\u05e8\u05ea \u05de\u05e8\u05d9\u05ea \u05e2\u05d3 \u05dc\u05e7\u05d1\u05dc\u05ea \u05d1\u05dc\u05d9\u05dc\u05d4 \u05d0\u05d7\u05d9\u05d3\u05d4 \u05dc\u05dc\u05d0 \u05d2\u05d5\u05e9\u05d9\u05dd. \u05d9\u05d5\u05e6\u05e7\u05d9\u05dd \u05d0\u05ea \u05d4\u05d1\u05dc\u05d9\u05dc\u05d4 \u05dc\u05ea\u05d1\u05e0\u05d9\u05ea \u05d5\u05de\u05d9\u05d9\u05e9\u05e8\u05d9\u05dd \u05d1\u05e2\u05d6\u05e8\u05ea \u05de\u05e8\u05d9\u05ea.  \u05d9\u05d5\u05e6\u05e7\u05d9\u05dd \u05e2\u05dc \u05d4\u05d1\u05e8\u05d0\u05d5\u05e0\u05d9\u05d6 \u05d1\u05e2\u05d6\u05e8\u05ea \u05db\u05e3 \u05ea\u05dc\u05d5\u05dc\u05d9\u05d5\u05ea \u05de\u05e7\u05e8\u05dd \u05d4\u05dc\u05d9\u05de\u05d5\u05df. \u05d1\u05e2\u05d6\u05e8\u05ea \u05e7\u05d9\u05e1\u05dd \u05d0\u05d5 \u05d9\u05d3\u05d9\u05ea \u05e9\u05dc \u05db\u05e3 \u05de\u05e2\u05e8\u05d1\u05d1\u05d9\u05dd \u05e7\u05dc\u05d5\u05ea \u05d0\u05ea \u05d1\u05dc\u05d9\u05dc\u05ea \u05d4\u05d1\u05e8\u05d0\u05d5\u05e0\u05d9\u05d6 \u05d5\u05d4\u05e7\u05e8\u05dd \u05dc\u05e7\u05d1\u05dc\u05ea \u05de\u05e8\u05d0\u05d4 \u05de\u05e9\u05d5\u05d9\u05e9.      7. \u05d0\u05d5\u05e4\u05d9\u05dd 30-20 \u05d3\u05e7\u05d5\u05ea, \u05e2\u05d3 \u05e9\u05e0\u05d5\u05e6\u05e8 \u05e2\u05dc \u05e4\u05e0\u05d9 \u05d4\u05d1\u05e8\u05d0\u05d5\u05e0\u05d9\u05d6 \u05e7\u05e8\u05d5\u05dd \u05e2\u05dd \u05e1\u05d3\u05e7\u05d9\u05dd \u05d3\u05e7\u05d9\u05dd, \u05d0\u05da \u05d1\u05e4\u05e0\u05d9\u05dd \u05d4\u05dd \u05e2\u05d3\u05d9\u05d9\u05df \u05dc\u05d7\u05d9\u05dd. \u05de\u05e6\u05e0\u05e0\u05d9\u05dd.  8. \u05d0\u05dd \u05e8\u05d5\u05e6\u05d9\u05dd, \u05de\u05d6\u05dc\u05e4\u05d9\u05dd \u05e2\u05dc \u05d4\u05d1\u05e8\u05d0\u05d5\u05e0\u05d9\u05d6 \u05e9\u05d5\u05e7\u05d5\u05dc\u05d3 \u05de\u05d5\u05de\u05e1. \u05e9\u05d5\u05de\u05e8\u05d9\u05dd \u05d1\u05de\u05e7\u05e8\u05e8 \u05d1\u05db\u05dc\u05d9 \u05d0\u05d8\u05d5\u05dd.   \u05e2\u05d5\u05d3 \u05de\u05ea\u05db\u05d5\u05e0\u05d9\u05dd \u05e9\u05dc \u05d0\u05d5\u05dc\u05d2\u05d4 \u05d8\u05d5\u05db\u05e9\u05e8 - \u05d1\u05d1\u05dc\u05d5\u05d2 \"\u05d4\u05de\u05e8\u05db\u05d9\u05d1 \u05d4\u05e1\u05d5\u05d3\u05d9\"      \u05d1\u05e8\u05d0\u05d5\u05e0\u05d9\u05d6 \u05e9\u05d5\u05e7\u05d5\u05dc\u05d3 \u05e2\u05dd \u05db\u05d3\u05d5\u05e8\u05d9 \u05e7\u05d5\u05e7\u05d5\u05e1 / \u05d0\u05d5\u05e8\u05dc\u05d9 \u05d7\u05e8\u05de\u05e9   \u05d1\u05e8\u05d0\u05d5\u05e0\u05d9\u05d6 \u05d3\u05d7\u05d5\u05e1\u05d9\u05dd, \u05e9\u05d5\u05e7\u05d5\u05dc\u05d3\u05d9\u05d9\u05dd \u05d5\u05e4\u05d0\u05d3\u05d2'\u05d9\u05d9\u05dd, \u05e9\u05d1\u05ea\u05d5\u05db\u05dd \u05db\u05d3\u05d5\u05e8\u05d9 \u05e7\u05d5\u05e7\u05d5\u05e1 \u05de\u05e4\u05ea\u05d9\u05e2\u05d9\u05dd. \u05d5\u05d0\u05dd \u05d6\u05d4 \u05dc\u05d0 \u05de\u05e1\u05e4\u05d9\u05e7 \u05dc\u05db\u05dd, \u05d0\u05ea\u05dd \u05de\u05d5\u05d6\u05de\u05e0\u05d9\u05dd \u05dc\u05d4\u05d5\u05e1\u05d9\u05e3 \u05dc\u05d7\u05d2\u05d9\u05d2\u05d4 \u05d2\u05dd \u05e8\u05d5\u05d8\u05d1 \u05d8\u05d5\u05e4\u05d9.    \u05de\u05e8\u05db\u05d9\u05d1\u05d9\u05dd  \u05ea\u05d1\u05e0\u05d9\u05ea \u05d1\u05e7\u05d5\u05d8\u05e8 24 \u05e1\"\u05de \u05d0\u05d5 \u05d1\u05d2\u05d5\u05d3\u05dc 20X20 \u05e1\"\u05de   \u05dc\u05db\u05d3\u05d5\u05e8\u05d9 \u05d4\u05e7\u05d5\u05e7\u05d5\u05e1  100 \u05d2\u05e8\u05dd \u05e9\u05d5\u05e7\u05d5\u05dc\u05d3 \u05dc\u05d1\u05df  25 \u05d2\u05e8\u05dd \u05d7\u05de\u05d0\u05d4  3/4 \u05db\u05d5\u05e1 (75 \u05d2\u05e8\u05dd) \u05e7\u05d5\u05e7\u05d5\u05e1 \u05d8\u05d7\u05d5\u05df  1 \u05db\u05e4\u05d9\u05ea \u05ea\u05de\u05e6\u05d9\u05ea \u05d5\u05e0\u05d9\u05dc \u05d0\u05d9\u05db\u05d5\u05ea\u05d9\u05ea   \u05dc\u05d1\u05e8\u05d0\u05d5\u05e0\u05d9\u05d6  100 \u05d2\u05e8\u05dd \u05e9\u05d5\u05e7\u05d5\u05dc\u05d3 \u05de\u05e8\u05d9\u05e8  130 \u05d2\u05e8\u05dd \u05d7\u05de\u05d0\u05d4  1/2 \u05db\u05e4\u05d9\u05ea \u05de\u05dc\u05d7  3 \u05db\u05e4\u05d5\u05ea (25 \u05d2\u05e8\u05dd) \u05d0\u05d1\u05e7\u05ea \u05e7\u05e7\u05d0\u05d5  2 \u05d1\u05d9\u05e6\u05d9\u05dd  3/4 \u05db\u05d5\u05e1 (180 \u05d2\u05e8\u05dd) \u05e1\u05d5\u05db\u05e8 \u05d7\u05d5\u05dd \u05db\u05d4\u05d4  1 \u05db\u05e4\u05d9\u05ea \u05ea\u05de\u05e6\u05d9\u05ea \u05d5\u05e0\u05d9\u05dc \u05d0\u05d9\u05db\u05d5\u05ea\u05d9\u05ea  1 \u05db\u05d5\u05e1 (140 \u05d2\u05e8\u05dd) \u05e7\u05de\u05d7 \u05e8\u05d2\u05d9\u05dc   \u05dc\u05e8\u05d5\u05d8\u05d1 \u05d4\u05d8\u05d5\u05e4\u05d9 (\u05dc\u05d0 \u05d7\u05d5\u05d1\u05d4)  30 \u05d2\u05e8\u05dd \u05d7\u05de\u05d0\u05d4  1/2 \u05db\u05d5\u05e1 (100 \u05d2\u05e8\u05dd) \u05e1\u05d5\u05db\u05e8 \u05d7\u05d5\u05dd \u05d3\u05de\u05e8\u05e8\u05d4  1/4 \u05db\u05e4\u05d9\u05ea \u05de\u05dc\u05d7  1 \u05db\u05e4\u05d9\u05ea \u05ea\u05de\u05e6\u05d9\u05ea \u05d5\u05e0\u05d9\u05dc \u05d0\u05d9\u05db\u05d5\u05ea\u05d9\u05ea  1/4 \u05db\u05d5\u05e1 (60 \u05de\"\u05dc) \u05d7\u05dc\u05d1  50 \u05d2\u05e8\u05dd \u05e9\u05d5\u05e7\u05d5\u05dc\u05d3 \u05d7\u05dc\u05d1   \u05d0\u05d5\u05e4\u05df \u05d4\u05d4\u05db\u05e0\u05d4    \u05db\u05d3\u05d5\u05e8\u05d9 \u05d4\u05e7\u05d5\u05e7\u05d5\u05e1  \u05de\u05de\u05d9\u05e1\u05d9\u05dd \u05d1\u05d9\u05d7\u05d3 \u05d0\u05ea \u05d4\u05e9\u05d5\u05e7\u05d5\u05dc\u05d3 \u05d4\u05dc\u05d1\u05df \u05d5\u05d4\u05d7\u05de\u05d0\u05d4 \u05d1\u05de\u05d9\u05e7\u05e8\u05d5\u05d2\u05dc \u05d0\u05d5 \u05d1\u05e1\u05d9\u05e8 \u05db\u05e4\u05d5\u05dc. \u05d1\u05de\u05d9\u05e7\u05e8\u05d5\u05d2\u05dc: \u05e9\u05de\u05d9\u05dd \u05d0\u05ea \u05d4\u05e9\u05d5\u05e7\u05d5\u05dc\u05d3 \u05d5\u05d4\u05d7\u05de\u05d0\u05d4 \u05d1\u05e7\u05e2\u05e8\u05d4, \u05de\u05d7\u05de\u05de\u05d9\u05dd 30 \u05e9\u05e0\u05d9\u05d5\u05ea, \u05de\u05e2\u05e8\u05d1\u05d1\u05d9\u05dd \u2013 \u05d5\u05d7\u05d5\u05d6\u05e8\u05d9\u05dd \u05e2\u05dc \u05d4\u05d7\u05d9\u05de\u05d5\u05dd \u05d4\u05e7\u05e6\u05e8 \u05d5\u05d4\u05e2\u05e8\u05d1\u05d5\u05d1 \u05e2\u05d3 \u05e9\u05d4\u05e9\u05d5\u05e7\u05d5\u05dc\u05d3 \u05e0\u05de\u05e1 \u05dc\u05d2\u05de\u05e8\u05d9. \u05d1\u05e1\u05d9\u05e8: \u05e9\u05de\u05d9\u05dd \u05d0\u05ea \u05d4\u05e9\u05d5\u05e7\u05d5\u05dc\u05d3 \u05d5\u05d4\u05d7\u05de\u05d0\u05d4 \u05d1\u05e1\u05d9\u05e8 \u05e7\u05d8\u05df (\u05d0\u05d5 \u05e7\u05e2\u05e8\u05ea \u05de\u05ea\u05db\u05ea), \u05d5\u05de\u05e0\u05d9\u05d7\u05d9\u05dd \u05d0\u05d5\u05ea\u05d5 \u05de\u05e2\u05dc \u05e1\u05d9\u05e8 \u05d2\u05d3\u05d5\u05dc \u05e9\u05d1\u05d5 \u05de\u05e2\u05d8 \u05de\u05d9\u05dd \u05e8\u05d5\u05ea\u05d7\u05d9\u05dd (\u05d0\u05e1\u05d5\u05e8 \u05e9\u05d4\u05de\u05d9\u05dd \u05d9\u05d9\u05d2\u05e2\u05d5 \u05d1\u05e1\u05d9\u05e8 \u05d4\u05e7\u05d8\u05df). \u05de\u05d7\u05de\u05de\u05d9\u05dd \u05ea\u05d5\u05da \u05d1\u05d7\u05d9\u05e9\u05d4, \u05e2\u05d3 \u05e9\u05d4\u05e9\u05d5\u05e7\u05d5\u05dc\u05d3 \u05e0\u05de\u05e1 \u05dc\u05d7\u05dc\u05d5\u05d8\u05d9\u05df.  \u05de\u05d5\u05e1\u05d9\u05e4\u05d9\u05dd \u05dc\u05e9\u05d5\u05e7\u05d5\u05dc\u05d3 \u05d4\u05de\u05d5\u05de\u05e1 \u05d0\u05ea \u05d4\u05e7\u05d5\u05e7\u05d5\u05e1 \u05d4\u05d8\u05d7\u05d5\u05df \u05d5\u05ea\u05de\u05e6\u05d9\u05ea \u05d4\u05d5\u05d5\u05e0\u05d9\u05dc \u05d5\u05de\u05e2\u05e8\u05d1\u05d1\u05d9\u05dd \u05d4\u05d9\u05d8\u05d1. \u05d9\u05d5\u05e6\u05e8\u05d9\u05dd \u05de\u05d4\u05ea\u05e2\u05e8\u05d5\u05d1\u05ea \u05db\u05d3\u05d5\u05e8\u05d9\u05dd \u05d1\u05d2\u05d5\u05d3\u05dc \u05e4\u05d9\u05e0\u05d2 \u05e4\u05d5\u05e0\u05d2, \u05de\u05e0\u05d9\u05d7\u05d9\u05dd \u05d1\u05e6\u05dc\u05d7\u05ea \u05d5\u05de\u05db\u05e1\u05d9\u05dd \u05d1\u05e0\u05d9\u05d9\u05dc\u05d5\u05df. \u05e9\u05d5\u05de\u05e8\u05d9\u05dd \u05d1\u05de\u05e7\u05e8\u05e8 \u05d1\u05d6\u05de\u05df \u05e9\u05de\u05db\u05d9\u05e0\u05d9\u05dd \u05d0\u05ea \u05ea\u05e2\u05e8\u05d5\u05d1\u05ea \u05d4\u05d1\u05e8\u05d0\u05d5\u05e0\u05d9\u05d6.   \u05d4\u05d1\u05e8\u05d0\u05d5\u05e0\u05d9\u05d6  \u05de\u05d7\u05de\u05de\u05d9\u05dd \u05d0\u05ea \u05d4\u05ea\u05e0\u05d5\u05e8 \u05dc-180 \u05de\u05e2\u05dc\u05d5\u05ea \u05d5\u05de\u05e8\u05e4\u05d3\u05d9\u05dd \u05d0\u05ea \u05d4\u05ea\u05d1\u05e0\u05d9\u05ea \u05d1\u05e0\u05d9\u05d9\u05e8 \u05d0\u05e4\u05d9\u05d9\u05d4.  \u05de\u05de\u05d9\u05e1\u05d9\u05dd \u05d9\u05d7\u05d3 \u05d0\u05ea \u05d4\u05e9\u05d5\u05e7\u05d5\u05dc\u05d3, \u05d4\u05d7\u05de\u05d0\u05d4, \u05d4\u05de\u05dc\u05d7 \u05d5\u05d0\u05d1\u05e7\u05ea \u05d4\u05e7\u05e7\u05d0\u05d5 \u05db\u05e4\u05d9 \u05e9\u05d4\u05de\u05e1\u05e0\u05d5 \u05d0\u05ea \u05d4\u05e9\u05d5\u05e7\u05d5\u05dc\u05d3 \u05d4\u05dc\u05d1\u05df \u05d5\u05d4\u05d7\u05de\u05d0\u05d4 \u05e2\u05d1\u05d5\u05e8 \u05db\u05d3\u05d5\u05e8\u05d9 \u05d4\u05e7\u05d5\u05e7\u05d5\u05e1.  \u05de\u05e7\u05e6\u05d9\u05e4\u05d9\u05dd \u05d9\u05d7\u05d3 \u05d0\u05ea \u05d4\u05d1\u05d9\u05e6\u05d9\u05dd, \u05d4\u05e1\u05d5\u05db\u05e8 \u05d5\u05ea\u05de\u05e6\u05d9\u05ea \u05d4\u05d5\u05d5\u05e0\u05d9\u05dc, \u05e2\u05d3 \u05e9\u05de\u05ea\u05e7\u05d1\u05dc\u05ea \u05ea\u05e2\u05e8\u05d5\u05d1\u05ea \u05ea\u05e4\u05d5\u05d7\u05d4 \u05d5\u05d1\u05d4\u05d9\u05e8\u05d4.  \u05de\u05e0\u05de\u05d9\u05db\u05d9\u05dd \u05d0\u05ea \u05de\u05d4\u05d9\u05e8\u05d5\u05ea \u05d4\u05de\u05d9\u05e7\u05e1\u05e8 \u05dc\u05de\u05d9\u05e0\u05d9\u05de\u05d5\u05dd \u05d5\u05de\u05d5\u05e1\u05d9\u05e4\u05d9\u05dd \u05dc\u05e7\u05e2\u05e8\u05d4 \u05d0\u05ea \u05ea\u05e2\u05e8\u05d5\u05d1\u05ea \u05d4\u05e9\u05d5\u05e7\u05d5\u05dc\u05d3. \u05db\u05e9\u05d4\u05d1\u05dc\u05d9\u05dc\u05d4 \u05d4\u05d5\u05e4\u05db\u05ea \u05dc\u05d0\u05d7\u05d9\u05d3\u05d4 \u05de\u05d5\u05e1\u05d9\u05e4\u05d9\u05dd \u05d0\u05ea \u05d4\u05e7\u05de\u05d7 \u05d1\u05e4\u05e2\u05dd \u05d0\u05d7\u05ea, \u05d5\u05de\u05de\u05e9\u05d9\u05db\u05d9\u05dd \u05dc\u05e2\u05e8\u05d1\u05dc \u05d1\u05d0\u05d8\u05d9\u05d5\u05ea \u05e8\u05e7 \u05e2\u05d3 \u05e9\u05de\u05ea\u05e7\u05d1\u05dc\u05ea \u05e9\u05d5\u05d1 \u05d1\u05dc\u05d9\u05dc\u05d4 \u05d0\u05d7\u05d9\u05d3\u05d4 (\u05e2\u05e8\u05d1\u05d5\u05dc \u05d9\u05ea\u05e8 \u05d9\u05e4\u05d2\u05e2 \u05d1\u05de\u05e8\u05e7\u05dd \u05d4\u05d1\u05e8\u05d0\u05d5\u05e0\u05d9\u05d6).  \u05d9\u05d5\u05e6\u05e7\u05d9\u05dd \u05dc\u05ea\u05d1\u05e0\u05d9\u05ea \u05e9\u05dc\u05d9\u05e9 \u05de\u05d4\u05d1\u05dc\u05d9\u05dc\u05d4 \u05d5\u05de\u05d9\u05d9\u05e9\u05e8\u05d9\u05dd. \u05de\u05e0\u05d9\u05d7\u05d9\u05dd \u05d0\u05ea \u05db\u05d3\u05d5\u05e8\u05d9 \u05d4\u05e7\u05d5\u05e7\u05d5\u05e1 \u05d1\u05ea\u05d1\u05e0\u05d9\u05ea.       6. \u05d9\u05d5\u05e6\u05e7\u05d9\u05dd \u05e2\u05dc \u05d4\u05db\u05d3\u05d5\u05e8\u05d9\u05dd \u05d0\u05ea \u05d9\u05ea\u05e8\u05ea \u05d4\u05d1\u05dc\u05d9\u05dc\u05d4 (\u05d6\u05d4 \u05d1\u05e1\u05d3\u05e8 \u05d0\u05dd \u05d4\u05d9\u05d0 \u05dc\u05d0 \u05de\u05db\u05e1\u05d4 \u05d0\u05d5\u05ea\u05dd \u05dc\u05d7\u05dc\u05d5\u05d8\u05d9\u05df, \u05db\u05da \u05e9\u05de\u05ea\u05e7\u05d1\u05dc\u05d5\u05ea \u05d2\u05d1\u05e2\u05d5\u05ea \u05e7\u05d8\u05e0\u05d5\u05ea)  7. \u05d0\u05d5\u05e4\u05d9\u05dd 35-30 \u05d3\u05e7\u05d5\u05ea, \u05e2\u05d3 \u05e9\u05e7\u05d9\u05e1\u05dd \u05d4\u05e0\u05e0\u05e2\u05e5 \u05d4\u05de\u05e8\u05db\u05d6 \u05d4\u05e2\u05d5\u05d2\u05d4 \u05d9\u05d5\u05e6\u05d0 \u05e2\u05dd \u05e4\u05d9\u05e8\u05d5\u05e8\u05d9\u05dd \u05dc\u05d7\u05d9\u05dd. \u05de\u05e6\u05e0\u05e0\u05d9\u05dd \u05dc\u05d8\u05de\u05e4\u05e8\u05d8\u05d5\u05e8\u05ea \u05d4\u05d7\u05d3\u05e8.   \u05e8\u05d5\u05d8\u05d1 \u05d4\u05d8\u05d5\u05e4\u05d9  \u05e9\u05de\u05d9\u05dd \u05d0\u05ea \u05db\u05dc \u05de\u05e8\u05db\u05d9\u05d1\u05d9 \u05d4\u05e8\u05d5\u05d8\u05d1 \u05d1\u05e1\u05d9\u05e8 \u05e7\u05d8\u05df \u05d5\u05de\u05d1\u05d9\u05d0\u05d9\u05dd \u05dc\u05e8\u05ea\u05d9\u05d7\u05d4 \u05e2\u05dc \u05d0\u05e9 \u05d1\u05d9\u05e0\u05d5\u05e0\u05d9\u05ea, \u05ea\u05d5\u05da \u05e2\u05e8\u05d1\u05d5\u05d1 \u05de\u05d3\u05d9 \u05e4\u05e2\u05dd. \u05de\u05e0\u05de\u05d9\u05db\u05d9\u05dd \u05d0\u05ea \u05d4\u05d0\u05e9 \u05d5\u05de\u05de\u05e9\u05d9\u05db\u05d9\u05dd \u05dc\u05d1\u05e9\u05dc \u05e2\u05d5\u05d3 \u05db\u05de\u05d4 \u05d3\u05e7\u05d5\u05ea, \u05ea\u05d5\u05da \u05e2\u05e8\u05d1\u05d5\u05d1, \u05e2\u05d3 \u05e9\u05d4\u05e8\u05d5\u05d8\u05d1 \u05de\u05e6\u05d8\u05de\u05e6\u05dd \u05d5\u05de\u05e1\u05de\u05d9\u05da (\u05d7\u05e9\u05d5\u05d1 \u05dc\u05d0 \u05dc\u05d1\u05e9\u05dc \u05d0\u05d5\u05ea\u05d5 \u05d9\u05d5\u05ea\u05e8 \u05de\u05d3\u05d9, \u05db\u05d3\u05d9 \u05e9\u05dc\u05d0 \u05d9\u05d9\u05e9\u05e8\u05e3).  \u05d9\u05d5\u05e6\u05e7\u05d9\u05dd \u05d0\u05ea \u05d4\u05e8\u05d5\u05d8\u05d1 \u05e2\u05dc \u05d4\u05e2\u05d5\u05d2\u05d4 \u05d1\u05e2\u05d5\u05d3\u05d5 \u05d7\u05dd (\u05d0\u05dd \u05d4\u05d5\u05d0 \u05de\u05ea\u05de\u05e6\u05e7 \u05d0\u05e4\u05e9\u05e8 \u05dc\u05d7\u05de\u05dd \u05d0\u05d5\u05ea\u05d5 \u05e9\u05d5\u05d1 \u05d1\u05e2\u05d3\u05d9\u05e0\u05d5\u05ea).  \u05e9\u05d5\u05de\u05e8\u05d9\u05dd \u05d1\u05de\u05e7\u05e8\u05e8 \u05d5\u05de\u05d2\u05d9\u05e9\u05d9\u05dd \u05d1\u05d8\u05de\u05e4\u05e8\u05d8\u05d5\u05e8\u05ea \u05d4\u05d7\u05d3\u05e8.   \u05e2\u05d5\u05d3 \u05de\u05ea\u05db\u05d5\u05e0\u05d9\u05dd \u05e9\u05dc \u05d0\u05d5\u05e8\u05dc\u05d9 \u05d7\u05e8\u05de\u05e9 - \u05d1\u05d1\u05dc\u05d5\u05d2 \"Brown Sugar\"       \u05d1\u05e8\u05d0\u05d5\u05e0\u05d9\u05d6 \u05e2\u05dd \u05d7\u05de\u05d0\u05ea \u05d1\u05d5\u05d8\u05e0\u05d9\u05dd \u05d5\u05e9\u05d5\u05e7\u05d5\u05dc\u05d3 \u05dc\u05d1\u05df / \u05d0\u05d9\u05e0\u05e1 \u05e9\u05d9\u05dc\u05ea \u05d9\u05e0\u05d0\u05d9   \u05de\u05e8\u05db\u05d9\u05d1\u05d9\u05dd  \u05ea\u05d1\u05e0\u05d9\u05ea \u05d1\u05d2\u05d5\u05d3\u05dc 20X30 \u05e1\"\u05de   200 \u05d2\u05e8\u05dd \u05e9\u05d5\u05e7\u05d5\u05dc\u05d3 \u05de\u05e8\u05d9\u05e8  180 \u05d2\u05e8\u05dd \u05d7\u05de\u05d0\u05d4  3/4 \u05db\u05d5\u05e1 (150 \u05d2\u05e8\u05dd) \u05e1\u05d5\u05db\u05e8  1/4 \u05db\u05e4\u05d9\u05ea \u05de\u05dc\u05d7  3 \u05d1\u05d9\u05e6\u05d9\u05dd  1/2 \u05db\u05d5\u05e1 (70 \u05d2\u05e8\u05dd) \u05e7\u05de\u05d7  3/4 \u05db\u05d5\u05e1 (100 \u05d2\u05e8\u05dd) \u05d0\u05d1\u05e7\u05ea \u05e7\u05e7\u05d0\u05d5  5 \u05db\u05e4\u05d5\u05ea \u05d7\u05de\u05d0\u05ea \u05d1\u05d5\u05d8\u05e0\u05d9\u05dd \u05e2\u05dd \u05d7\u05ea\u05d9\u05db\u05d5\u05ea \u05d1\u05d5\u05d8\u05e0\u05d9\u05dd  100 \u05d2\u05e8\u05dd \u05de\u05d8\u05d1\u05e2\u05d5\u05ea \u05e9\u05d5\u05e7\u05d5\u05dc\u05d3 \u05dc\u05d1\u05df, \u05d0\u05d5 \u05d8\u05d1\u05dc\u05ea \u05e9\u05d5\u05e7\u05d5\u05dc\u05d3 \u05dc\u05d1\u05df \u05e9\u05d1\u05d5\u05e8\u05d4 \u05dc\u05e7\u05d5\u05d1\u05d9\u05d5\u05ea   \u05d0\u05d5\u05e4\u05df \u05d4\u05d4\u05db\u05e0\u05d4  \u05de\u05d7\u05de\u05de\u05d9\u05dd \u05d0\u05ea \u05d4\u05ea\u05e0\u05d5\u05e8 \u05dc-180 \u05de\u05e2\u05dc\u05d5\u05ea. \u05de\u05e8\u05e4\u05d3\u05d9\u05dd \u05d0\u05ea \u05d4\u05ea\u05d1\u05e0\u05d9\u05ea \u05d1\u05e0\u05d9\u05d9\u05e8 \u05d0\u05e4\u05d9\u05d9\u05d4.  \u05de\u05de\u05d9\u05e1\u05d9\u05dd \u05d9\u05d7\u05d3 \u05d1\u05de\u05d9\u05e7\u05e8\u05d5\u05d2\u05dc \u05d0\u05d5 \u05d1\u05d1\u05df \u05de\u05d0\u05e8\u05d9 \u05d0\u05ea \u05d4\u05e9\u05d5\u05e7\u05d5\u05dc\u05d3 \u05d4\u05de\u05e8\u05d9\u05e8, \u05d4\u05d7\u05de\u05d0\u05d4, \u05d4\u05de\u05dc\u05d7 \u05d5\u05d4\u05e1\u05d5\u05db\u05e8. \u05de\u05d5\u05e1\u05d9\u05e4\u05d9\u05dd \u05d0\u05ea \u05d4\u05d1\u05d9\u05e6\u05d9\u05dd \u05d1\u05d6\u05d5 \u05d0\u05d7\u05e8 \u05d6\u05d5, \u05ea\u05d5\u05da \u05d8\u05e8\u05d9\u05e4\u05d4.  \u05de\u05e2\u05e8\u05d1\u05d1\u05d9\u05dd \u05d0\u05ea \u05d4\u05e7\u05de\u05d7 \u05d5\u05d4\u05e7\u05e7\u05d0\u05d5, \u05de\u05d5\u05e1\u05d9\u05e4\u05d9\u05dd \u05dc\u05ea\u05e2\u05e8\u05d5\u05d1\u05ea \u05d4\u05e9\u05d5\u05e7\u05d5\u05dc\u05d3 \u05d5\u05de\u05e2\u05e8\u05d1\u05d1\u05d9\u05dd \u05e2\u05d3 \u05e8\u05e7 \u05e2\u05d3 \u05dc\u05d0\u05d9\u05d7\u05d5\u05d3.  \u05de\u05d7\u05de\u05de\u05d9\u05dd \u05d0\u05ea \u05d7\u05de\u05d0\u05ea \u05d4\u05d1\u05d5\u05d8\u05e0\u05d9\u05dd \u05d1\u05de\u05d9\u05e7\u05e8\u05d5\u05d2\u05dc \u05d1\u05de\u05e9\u05da \u05db\u05de\u05d4 \u05e9\u05e0\u05d9\u05d5\u05ea, \u05e2\u05d3 \u05e9\u05d4\u05d9\u05d0 \u05d4\u05d5\u05e4\u05db\u05ea \u05dc\u05e0\u05d5\u05d6\u05dc\u05d9\u05ea.  \u05de\u05e2\u05d1\u05d9\u05e8\u05d9\u05dd \u05de\u05d7\u05e6\u05d9\u05ea \u05de\u05ea\u05e2\u05e8\u05d5\u05d1\u05ea \u05d4\u05e9\u05d5\u05e7\u05d5\u05dc\u05d3 \u05dc\u05ea\u05d1\u05e0\u05d9\u05ea \u05d5\u05d9\u05d5\u05e6\u05e7\u05d9\u05dd \u05de\u05e2\u05dc \u05de\u05d7\u05e6\u05d9\u05ea \u05de\u05d7\u05de\u05d0\u05ea \u05d4\u05d1\u05d5\u05d8\u05e0\u05d9\u05dd. \u05de\u05e2\u05e8\u05d1\u05d1\u05d9\u05dd \u05e7\u05dc\u05d5\u05ea \u05e2\u05dd \u05de\u05d6\u05dc\u05d2, \u05dc\u05d9\u05e6\u05d9\u05e8\u05ea \u05d3\u05d5\u05d2\u05de\u05ea \u05e9\u05d9\u05e9, \u05d5\u05de\u05e1\u05d3\u05e8\u05d9\u05dd \u05de\u05e2\u05dc \u05de\u05d7\u05e6\u05d9\u05ea \u05de\u05d4\u05e9\u05d5\u05e7\u05d5\u05dc\u05d3 \u05d4\u05dc\u05d1\u05df. \u05d9\u05d5\u05e6\u05e7\u05d9\u05dd \u05dc\u05ea\u05d1\u05e0\u05d9\u05ea \u05d0\u05ea \u05d9\u05ea\u05e8\u05ea \u05ea\u05e2\u05e8\u05d5\u05d1\u05ea \u05d4\u05e9\u05d5\u05e7\u05d5\u05dc\u05d3 \u05d5\u05de\u05e1\u05d3\u05e8\u05d9\u05dd \u05de\u05e2\u05dc \u05d0\u05ea \u05d9\u05ea\u05e8\u05ea \u05d4\u05e9\u05d5\u05e7\u05d5\u05dc\u05d3 \u05d4\u05dc\u05d1\u05df. \u05d1\u05e2\u05d6\u05e8\u05ea \u05db\u05e4\u05d9\u05ea \u05de\u05e4\u05d6\u05e8\u05d9\u05dd \u05d0\u05ea \u05d9\u05ea\u05e8\u05ea \u05d7\u05de\u05d0\u05ea \u05d4\u05d1\u05d5\u05d8\u05e0\u05d9\u05dd \u05e2\u05dc \u05e4\u05e0\u05d9 \u05d4\u05e2\u05d5\u05d2\u05d4.  \u05d0\u05d5\u05e4\u05d9\u05dd \u05db-25 \u05d3\u05e7\u05d5\u05ea, \u05e2\u05d3 \u05e9\u05e7\u05d9\u05e1\u05dd \u05d4\u05e0\u05e0\u05e2\u05e5 \u05d1\u05de\u05e8\u05db\u05d6 \u05d4\u05e2\u05d5\u05d2\u05d4 \u05d9\u05d5\u05e6\u05d0 \u05dc\u05d7 (\u05d4\u05e2\u05d5\u05d2\u05d4 \u05ea\u05de\u05e9\u05d9\u05da \u05dc\u05d4\u05ea\u05d9\u05d9\u05e6\u05d1 \u05db\u05e9\u05ea\u05ea\u05e7\u05e8\u05e8). \u05de\u05e6\u05e0\u05e0\u05d9\u05dd \u05d5\u05d7\u05d5\u05ea\u05db\u05d9\u05dd \u05dc\u05e7\u05d5\u05d1\u05d9\u05d5\u05ea.   \u05e2\u05d5\u05d3 \u05de\u05ea\u05db\u05d5\u05e0\u05d9\u05dd \u05e9\u05dc \u05d0\u05d9\u05e0\u05e1 \u05e9\u05d9\u05dc\u05ea \u05d9\u05e0\u05d0\u05d9 - \u05d1\u05d1\u05dc\u05d5\u05d2 \"\u05ea\u05d9\u05e7 \u05d0\u05d5\u05db\u05dc\"       \u05d1\u05e8\u05d0\u05d5\u05e0\u05d9\u05d6 \u05e9\u05d5\u05e7\u05d5\u05dc\u05d3 \u05d5\u05e4\u05e8\u05d2 \u05dc\u05dc\u05d0 \u05d2\u05dc\u05d5\u05d8\u05df / \u05d0\u05e4\u05e8\u05ea \u05e1\u05d9\u05d0\u05e6'\u05d9    \u05de\u05e8\u05db\u05d9\u05d1\u05d9\u05dd  \u05ea\u05d1\u05e0\u05d9\u05ea \u05d1\u05d2\u05d5\u05d3\u05dc 20X30 \u05e1\"\u05de   100 \u05d2\u05e8\u05dd \u05e9\u05d5\u05e7\u05d5\u05dc\u05d3 \u05de\u05e8\u05d9\u05e8  2/3 \u05db\u05d5\u05e1 (160 \u05de\"\u05dc) \u05e9\u05de\u05df  4 \u05d1\u05d9\u05e6\u05d9\u05dd  1 \u05db\u05d5\u05e1 (200 \u05d2\u05e8\u05dd) \u05e1\u05d5\u05db\u05e8  1 \u05db\u05d5\u05e1 (100 \u05d2\u05e8\u05dd) \u05e4\u05e8\u05d2 \u05d8\u05d7\u05d5\u05df \u05d8\u05e8\u05d9  1/4 \u05db\u05d5\u05e1 (35 \u05d2\u05e8\u05dd) \u05d0\u05d1\u05e7\u05ea \u05e7\u05e7\u05d0\u05d5  1/2 \u05db\u05e4\u05d9\u05ea \u05d0\u05d1\u05e7\u05ea \u05d0\u05e4\u05d9\u05d9\u05d4   \u05d0\u05d5\u05e4\u05df \u05d4\u05d4\u05db\u05e0\u05d4  \u05de\u05d7\u05de\u05de\u05d9\u05dd \u05d0\u05ea \u05d4\u05ea\u05e0\u05d5\u05e8 \u05dc-180 \u05de\u05e2\u05dc\u05d5\u05ea. \u05de\u05e9\u05de\u05e0\u05d9\u05dd \u05d0\u05ea \u05d4\u05ea\u05d1\u05e0\u05d9\u05ea.  \u05de\u05de\u05d9\u05e1\u05d9\u05dd \u05d1\u05de\u05d9\u05e7\u05e8\u05d5\u05d2\u05dc \u05d0\u05ea \u05d4\u05e9\u05d5\u05e7\u05d5\u05dc\u05d3 \u05e2\u05dd \u05d4\u05e9\u05de\u05df. \u05de\u05e2\u05e8\u05d1\u05d1\u05d9\u05dd \u05dc\u05ea\u05e2\u05e8\u05d5\u05d1\u05ea \u05d0\u05d7\u05d9\u05d3\u05d4 \u05d5\u05de\u05e6\u05e0\u05e0\u05d9\u05dd.  \u05de\u05e7\u05e6\u05d9\u05e4\u05d9\u05dd \u05d0\u05ea \u05d4\u05d1\u05d9\u05e6\u05d9\u05dd \u05e2\u05dd \u05d4\u05e1\u05d5\u05db\u05e8 \u05d1\u05de\u05e9\u05da \u05db\u05d3\u05e7\u05d4. \u05de\u05d5\u05e1\u05d9\u05e4\u05d9\u05dd \u05d0\u05ea \u05ea\u05e2\u05e8\u05d5\u05d1\u05ea \u05d4\u05e9\u05d5\u05e7\u05d5\u05dc\u05d3 \u05d5\u05de\u05e7\u05e6\u05d9\u05e4\u05d9\u05dd \u05e8\u05e7 \u05e2\u05d3 \u05e9\u05d4\u05d9\u05d0 \u05e0\u05d8\u05de\u05e2\u05ea \u05d1\u05e7\u05e6\u05d9\u05e4\u05d4.  \u05de\u05e2\u05e8\u05d1\u05d1\u05d9\u05dd \u05d1\u05e7\u05e2\u05e8\u05d9\u05ea \u05d0\u05ea \u05d4\u05e4\u05e8\u05d2 \u05e2\u05dd \u05d4\u05e7\u05e7\u05d0\u05d5 \u05d5\u05d0\u05d1\u05e7\u05ea \u05d4\u05d0\u05e4\u05d9\u05d9\u05d4 \u05d5\u05de\u05d5\u05e1\u05d9\u05e4\u05d9\u05dd \u05dc\u05ea\u05e2\u05e8\u05d5\u05d1\u05ea. \u05de\u05e2\u05e8\u05d1\u05d1\u05d9\u05dd \u05dc\u05ea\u05e2\u05e8\u05d5\u05d1\u05ea \u05d0\u05d7\u05d9\u05d3\u05d4 \u05d5\u05d9\u05d5\u05e6\u05e7\u05d9\u05dd \u05dc\u05ea\u05d1\u05e0\u05d9\u05ea.  \u05d0\u05d5\u05e4\u05d9\u05dd \u05db-25 \u05d3\u05e7\u05d5\u05ea, \u05e2\u05d3 \u05e9\u05e7\u05d9\u05e1\u05dd \u05d4\u05e0\u05e0\u05e2\u05e5 \u05d1\u05de\u05e8\u05db\u05d6 \u05d4\u05e2\u05d5\u05d2\u05d4 \u05d9\u05d5\u05e6\u05d0 \u05e2\u05dd \u05e4\u05d9\u05e8\u05d5\u05e8\u05d9\u05dd \u05dc\u05d7\u05d9\u05dd (\u05d4\u05e2\u05d5\u05d2\u05d4 \u05ea\u05d4\u05d9\u05d4 \u05e7\u05e6\u05ea \u05e0\u05d5\u05de\u05d5\u05db\u05d4, \u05d5\u05d6\u05d4 \u05d1\u05e1\u05d3\u05e8). \u05de\u05e0\u05d9\u05d7\u05d9\u05dd \u05dc\u05e2\u05d5\u05d2\u05d4 \u05dc\u05d4\u05ea\u05e7\u05e8\u05e8 \u05dc\u05d2\u05de\u05e8\u05d9 \u05d1\u05ea\u05d1\u05e0\u05d9\u05ea \u05d5\u05d7\u05d5\u05ea\u05db\u05d9\u05dd \u05dc\u05e8\u05d9\u05d1\u05d5\u05e2\u05d9\u05dd.   \u05e2\u05d5\u05d3 \u05de\u05ea\u05db\u05d5\u05e0\u05d9\u05dd \u05e9\u05dc \u05d0\u05e4\u05e8\u05ea \u05e1\u05d9\u05d0\u05e6'\u05d9 - \u05d1\u05d0\u05ea\u05e8 \"\u05de\u05ea\u05db\u05d5\u05e0\u05d9\u05dd \u05d1-10 \u05d3\u05e7\u05d5\u05ea\"       \u05d1\u05e8\u05d0\u05d5\u05e0\u05d9\u05d6 \u05e2\u05dd \u05d2\u05d1\u05e2\u05d5\u05ea \u05e9\u05d5\u05e7\u05d5\u05dc\u05d3 \u05d5\u05de\u05dc\u05d7 \u05d0\u05d8\u05dc\u05e0\u05d8\u05d9 / \u05d0\u05d9\u05e0\u05e1 \u05e9\u05d9\u05dc\u05ea \u05d9\u05e0\u05d0\u05d9    \u05de\u05e8\u05db\u05d9\u05d1\u05d9\u05dd \u05ea\u05d1\u05e0\u05d9\u05ea \u05d1\u05d2\u05d5\u05d3\u05dc 22X23 \u05e1\"\u05de   50 \u05d2\u05e8\u05dd \u05e9\u05d5\u05e7\u05d5\u05dc\u05d3 \u05d7\u05dc\u05d1  250 \u05d2\u05e8\u05dd \u05e9\u05d5\u05e7\u05d5\u05dc\u05d3 \u05de\u05e8\u05d9\u05e8  1/2 \u05db\u05d5\u05e1 (100 \u05d2\u05e8\u05dd) \u05e1\u05d5\u05db\u05e8  180 \u05d2\u05e8\u05dd \u05d7\u05de\u05d0\u05d4  1 \u05db\u05e4\u05d9\u05ea \u05ea\u05de\u05e6\u05d9\u05ea \u05d5\u05e0\u05d9\u05dc  \u05e7\u05d5\u05e8\u05d8 \u05de\u05dc\u05d7  3 \u05d1\u05d9\u05e6\u05d9\u05dd  2/3 \u05db\u05d5\u05e1 (95 \u05d2\u05e8\u05dd) \u05e7\u05de\u05d7 \u05dc\u05d1\u05df  4 \u05db\u05e4\u05d5\u05ea \u05de\u05dc\u05d0\u05d5\u05ea \u05d0\u05d1\u05e7\u05ea \u05e7\u05e7\u05d0\u05d5 \u05de\u05e9\u05d5\u05d1\u05d7\u05ea  1 \u05db\u05e4\u05d9\u05ea \u05de\u05dc\u05d7 \u05d0\u05d8\u05dc\u05e0\u05d8\u05d9 \u05d2\u05e1   \u05d0\u05d5\u05e4\u05df \u05d4\u05d4\u05db\u05e0\u05d4  \u05de\u05d7\u05de\u05de\u05d9\u05dd \u05d0\u05ea \u05d4\u05ea\u05e0\u05d5\u05e8 \u05dc-170 \u05de\u05e2\u05dc\u05d5\u05ea.  \u05e9\u05d5\u05d1\u05e8\u05d9\u05dd \u05dc\u05d7\u05ea\u05d9\u05db\u05d5\u05ea \u05d1\u05d9\u05e0\u05d5\u05e0\u05d9\u05d5\u05ea \u05d0\u05ea \u05e9\u05d5\u05e7\u05d5\u05dc\u05d3 \u05d4\u05d7\u05dc\u05d1 \u05d5-50 \u05d2\u05e8\u05dd \u05de\u05d4\u05e9\u05d5\u05e7\u05d5\u05dc\u05d3 \u05d4\u05de\u05e8\u05d9\u05e8. \u05e9\u05d5\u05de\u05e8\u05d9\u05dd \u05d1\u05e6\u05d3.  \u05de\u05de\u05d9\u05e1\u05d9\u05dd \u05d9\u05d7\u05d3, \u05d1\u05e1\u05d9\u05e8 \u05db\u05e4\u05d5\u05dc \u05d0\u05d5 \u05d1\u05de\u05d9\u05e7\u05e8\u05d5\u05d2\u05dc, \u05d0\u05ea \u05d9\u05ea\u05e8\u05ea \u05d4\u05e9\u05d5\u05e7\u05d5\u05dc\u05d3 \u05d4\u05de\u05e8\u05d9\u05e8, \u05d4\u05e1\u05d5\u05db\u05e8 \u05d5\u05d4\u05d7\u05de\u05d0\u05d4. \u05de\u05d5\u05e1\u05d9\u05e4\u05d9\u05dd \u05d0\u05ea \u05ea\u05de\u05e6\u05d9\u05ea \u05d4\u05d5\u05d5\u05e0\u05d9\u05dc \u05d5\u05e7\u05d5\u05e8\u05d8 \u05de\u05dc\u05d7 \u05d5\u05de\u05e2\u05e8\u05d1\u05d1\u05d9\u05dd \u05d4\u05d9\u05d8\u05d1. \u05de\u05d5\u05e1\u05d9\u05e4\u05d9\u05dd \u05d2\u05dd \u05d0\u05ea \u05d4\u05d1\u05d9\u05e6\u05d9\u05dd, \u05d1\u05d6\u05d5 \u05d0\u05d7\u05e8 \u05d6\u05d5 \u05d5\u05ea\u05d5\u05da \u05d1\u05d7\u05d9\u05e9\u05d4. \u05de\u05d5\u05e1\u05d9\u05e4\u05d9\u05dd \u05d0\u05ea \u05d4\u05e7\u05de\u05d7 \u05d5\u05d4\u05e7\u05e7\u05d0\u05d5 \u05d5\u05d1\u05d5\u05d7\u05e9\u05d9\u05dd \u05e8\u05e7 \u05e2\u05d3 \u05e9\u05de\u05ea\u05e7\u05d1\u05dc\u05ea \u05d1\u05dc\u05d9\u05dc\u05d4 \u05d0\u05d7\u05d9\u05d3\u05d4. \u05dc\u05d1\u05e1\u05d5\u05e3 \u05de\u05d5\u05e1\u05d9\u05e4\u05d9\u05dd \u05de\u05d7\u05e6\u05d9\u05ea \u05de\u05db\u05de\u05d5\u05ea \u05d4\u05e9\u05d5\u05e7\u05d5\u05dc\u05d3 \u05d4\u05e9\u05d1\u05d5\u05e8 (\u05d7\u05dc\u05d1 \u05d5\u05de\u05e8\u05d9\u05e8) \u05d5\u05de\u05e2\u05e8\u05d1\u05d1\u05d9\u05dd \u05e7\u05e6\u05e8\u05d5\u05ea.  \u05de\u05e9\u05de\u05e0\u05d9\u05dd \u05e7\u05dc\u05d5\u05ea \u05d0\u05ea \u05d4\u05ea\u05d1\u05e0\u05d9\u05ea \u05d5\u05d9\u05d5\u05e6\u05e7\u05d9\u05dd \u05dc\u05ea\u05d5\u05db\u05d4 \u05d0\u05ea \u05d4\u05d1\u05dc\u05d9\u05dc\u05d4. \u05de\u05e4\u05d6\u05e8\u05d9\u05dd \u05de\u05e2\u05dc \u05d0\u05ea \u05d9\u05ea\u05e8\u05ea \u05d4\u05e9\u05d5\u05e7\u05d5\u05dc\u05d3 \u05d4\u05e9\u05d1\u05d5\u05e8 \u05d5\u05d3\u05d5\u05d7\u05e4\u05d9\u05dd \u05de\u05e2\u05d8 \u05d0\u05ea \u05d4\u05e9\u05d1\u05e8\u05d9\u05dd \u05dc\u05ea\u05d5\u05da \u05d4\u05d1\u05dc\u05d9\u05dc\u05d4. \u05de\u05e4\u05d6\u05e8\u05d9\u05dd \u05de\u05e2\u05dc \u05d0\u05ea \u05d4\u05de\u05dc\u05d7 \u05d4\u05d2\u05e1.  \u05d0\u05d5\u05e4\u05d9\u05dd 30-25 \u05d3\u05e7\u05d5\u05ea \u05d5\u05dc\u05d0 \u05d9\u05d5\u05ea\u05e8. \u05de\u05e6\u05e0\u05e0\u05d9\u05dd \u05d5\u05d7\u05d5\u05ea\u05db\u05d9\u05dd \u05dc\u05e4\u05e8\u05d5\u05e1\u05d5\u05ea.   \u05e2\u05d5\u05d3 \u05de\u05ea\u05db\u05d5\u05e0\u05d9\u05dd \u05e9\u05dc \u05d0\u05d9\u05e0\u05e1 \u05e9\u05d9\u05dc\u05ea \u05d9\u05e0\u05d0\u05d9 - \u05d1\u05d1\u05dc\u05d5\u05d2 \"\u05ea\u05d9\u05e7 \u05d0\u05d5\u05db\u05dc\"   --------------------------------------------------------------------------------------------------   \u05d5\u05de\u05d4 \u05dc\u05d2\u05d1\u05d9 \u05e2\u05d5\u05d2\u05d5\u05ea \u05e0\u05d5\u05d8\u05dc\u05d4 \u05d0\u05d9\u05e9\u05d9\u05d5\u05ea \u05de-3 \u05de\u05e8\u05db\u05d9\u05d1\u05d9\u05dd \u05d1\u05dc\u05d1\u05d3? \u05dc\u05d7\u05e6\u05d5 \u05e2\u05dc \u05d4\u05ea\u05de\u05d5\u05e0\u05d4 \u05db\u05d3\u05d9 \u05dc\u05e2\u05d1\u05d5\u05e8 \u05dc\u05de\u05ea\u05db\u05d5\u05df    ", "title": "5 \u05d1\u05e8\u05d0\u05d5\u05e0\u05d9\u05d6 \u05de\u05d9\u05d5\u05d7\u05d3\u05d9\u05dd \u05d1\u05de\u05d9\u05e0\u05dd: \u05de\u05ea\u05db\u05d5\u05e0\u05d9\u05dd\n\u05de\u05e9\u05d5\u05d3\u05e8\u05d2\u05d9\u05dd \u05dc\u05e2\u05d5\u05d2\u05d4 \u05e9\u05db-\u05d5-\u05dc-\u05dd \u05d0\u05d5\u05d4\u05d1\u05d9\u05dd", "url": "http://xnet.ynet.co.il/articles/0,7340,L-4892042,00.html", "type": "article", "site_name": "xnet", "published_time": "", "category": "", "subtitle": ""}