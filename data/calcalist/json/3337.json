5db\u05da \u05e9\u05e2\u05d3 \u05e1\u05d5\u05e3 \u05d4\u05e9\u05d1\u05d5\u05e2 \u05ea\u05d5\u05db\u05e0\u05d9\u05d5\u05ea \u05d4\u05e7\u05dc\u05d4 \u05e4\u05d9\u05e1\u05e7\u05dc\u05d9\u05ea \u05d5\u05de\u05d5\u05e0\u05d9\u05d8\u05e8\u05d9\u05ea \u05de\u05e1\u05d9\u05d1\u05d9\u05d5\u05ea \u05d9\u05e1\u05d9\u05d9\u05e2\u05d5 \u05dc\u05d4\u05e8\u05d2\u05d9\u05e2 \u05d0\u05ea \u05d4\u05e9\u05d5\u05d5\u05e7\u05d9\u05dd. \u05d0\u05dd \u05d4\u05ea\u05e0\u05d5\u05d3\u05ea\u05d9\u05d5\u05ea \u05d1\u05e9\u05d5\u05e7 \u05ea\u05ea\u05d9\u05d9\u05e6\u05d1 \u05de\u05e2\u05d8, \u05d4\u05d6\u05d4\u05d1 \u05d9\u05d7\u05d6\u05d5\u05e8 \u05dc\u05de\u05e2\u05de\u05d3\u05d5 \u05db\u05e0\u05db\u05e1 \u05d1\u05d8\u05d5\u05d7&quot;, \u05d4\u05d5\u05e1\u05d9\u05e3 \u05de\u05d5\u05d9\u05d4.\r\n \r\n \r\n  \r\n11:30 - \u05de\u05d3\u05d3 \u05de\u05e0\u05d4\u05dc\u05d9 \u05d4\u05e8\u05db\u05e9 \u05d4\u05de\u05e9\u05d5\u05e7\u05dc\u05dc \u05d1\u05d2\u05d5\u05e9 \u05d4\u05d9\u05d5\u05e8\u05d5 \u05e6\u05e0\u05d7 \u05d1\u05d7\u05d5\u05d3\u05e9 \u05de\u05e8\u05e5 \u05dc\u05e8\u05de\u05ea\u05d5 \u05d4\u05e0\u05de\u05d5\u05db\u05d4 \u05d1\u05d9\u05d5\u05ea\u05e8 \u05d0\u05d9 \u05e4\u05e2\u05dd. \u05d4\u05de\u05d3\u05d3 \u05e9\u05de\u05e4\u05e8\u05e1\u05de\u05ea \u05d7\u05d1\u05e8\u05ea Markit \u05e6\u05e0\u05d7 \u05dc-31.4 \u05e0\u05e7\u05d5\u05d3\u05d5\u05ea \u05dc\u05e2\u05d5\u05de\u05ea 51.6 \u05e0\u05e7\u05d5\u05d3\u05d5\u05ea \u05d1\u05e4\u05d1\u05e8\u05d5\u05d0\u05e8, \u05d5\u05dc\u05d0 \u05e2\u05de\u05d3 \u05d1\u05ea\u05d7\u05d6\u05d9\u05d5\u05ea \u05d4\u05d0\u05e0\u05dc\u05d9\u05e1\u05d8\u05d9\u05dd \u05dc\u05e7\u05e8\u05d9\u05d0\u05d4 \u05e9\u05dc 38.8 \u05e0\u05e7\u05d5\u05d3\u05d5\u05ea. \u05de\u05d3\u05d5\u05d1\u05e8 \u05d1\u05e9\u05e4\u05dc \u05de\u05d0\u05d6 \u05ea\u05d7\u05d9\u05dc\u05ea \u05e4\u05e8\u05e1\u05d5\u05dd \u05d4\u05e0\u05ea\u05d5\u05e0\u05d9\u05dd \u05d1\u05d9\u05d5\u05dc\u05d9 1998.\r\n \r\n\u05de\u05d3\u05d3 \u05de\u05e0\u05d4\u05dc\u05d9 \u05d4\u05e8\u05db\u05e9 \u05e9\u05dc Markit \u05d1\u05ea\u05d7\u05d5\u05dd \u05d4\u05e9\u05d9\u05e8\u05d5\u05ea\u05d9\u05dd, \u05e9\u05e0\u05d5\u05e2\u05d3 \u05dc\u05e9\u05e7\u05e3 \u05d0\u05ea \u05e8\u05de\u05ea \u05d4\u05e8\u05db\u05d9\u05e9\u05d4 \u05e9\u05dc \u05de\u05e0\u05d4\u05dc\u05d9 \u05d4\u05e9\u05d9\u05e8\u05d5\u05ea\u05d9\u05dd \u05d1\u05de\u05d2\u05d6\u05e8 \u05d4\u05ea\u05e2\u05e9\u05d9\u05d9\u05ea\u05d9, \u05e0\u05e4\u05dc \u05dc-28.4 \u05e0\u05e7\u05d5\u05d3\u05d5\u05ea \u05dc\u05e2\u05d5\u05de\u05ea 52.6 \u05d1\u05e4\u05d1\u05e8\u05d5\u05d0\u05e8, \u05d5\u05d0\u05d9\u05db\u05d6\u05d1 \u05d0\u05ea \u05d4\u05e6\u05d9\u05e4\u05d9\u05d5\u05ea \u05dc-39 \u05e0\u05e7\u05d5\u05d3\u05d5\u05ea.\r\n \r\n\u05de\u05d3\u05d3 \u05de\u05e0\u05d4\u05dc\u05d9 \u05d4\u05d2\u05e9\u05e8 \u05d1\u05de\u05d2\u05d6\u05e8 \u05d4\u05d9\u05e6\u05e8\u05e0\u05d9 \u05e0\u05d7\u05dc\u05e9 \u05dc-44.8% \u05dc\u05e2\u05d5\u05de\u05ea 49.2 \u05d1\u05d7\u05d5\u05d3\u05e9 \u05d4\u05e7\u05d5\u05d3\u05dd, \u05d0\u05da \u05e2\u05dc\u05d4 \u05e2\u05dc \u05d4\u05e6\u05d9\u05e4\u05d9\u05d5\u05ea \u05dc-39 \u05e0\u05e7\u05d5\u05d3\u05d5\u05ea. \u05e0\u05ea\u05d5\u05df \u05e0\u05de\u05d5\u05da \u05de-50 \u05e0\u05e7\u05d5\u05d3\u05d5\u05ea \u05de\u05e6\u05d1\u05d9\u05e2 \u05e2\u05dc \u05d4\u05ea\u05db\u05d5\u05d5\u05e6\u05d5\u05ea \u05d1\u05e2\u05e0\u05e3.\r\n \r\n10:10 - \u05d2\u05dd \u05d4\u05d1\u05d5\u05e8\u05e1\u05d5\u05ea \u05d1\u05d0\u05d9\u05e8\u05d5\u05e4\u05d4 \u05e4\u05d5\u05ea\u05d7\u05d5\u05ea \u05d0\u05ea \u05d9\u05d5\u05dd \u05d4\u05de\u05e1\u05d7\u05e8 \u05d1\u05de\u05d2\u05de\u05d4 \u05d7\u05d9\u05d5\u05d1\u05d9\u05ea \u05d7\u05d6\u05e7\u05d4, \u05d1\u05e8\u05e7\u05e2 \u05ea\u05d5\u05db\u05e0\u05d9\u05ea \u05d4\u05d4\u05e7\u05dc\u05d4 \u05e9\u05dc \u05d4\u05e4\u05d3 \u05d4\u05d0\u05de\u05e8\u05d9\u05e7\u05d0\u05d9. \u05de\u05d3\u05d3 \u05d3\u05d0\u05e7\u05e1 \u05d1\u05e4\u05e8\u05e0\u05e7\u05e4\u05d5\u05e8\u05d8 \u05de\u05d6\u05e0\u05e7 \u05d1-6.1%, \u05e7\u05d0\u05e7 \u05d4\u05e4\u05e8\u05d9\u05d6\u05d0\u05d9 \u05e7\u05d5\u05e4\u05e5 \u05d1-4.6% \u05d5\u05e4\u05d5\u05d8\u05e1\u05d9 \u05d4\u05dc\u05d5\u05e0\u05d3\u05d5\u05e0\u05d9 \u05de\u05d5\u05e1\u05d9\u05e3 \u05dc\u05e2\u05e8\u05db\u05d5 3.6%.\r\n \r\n\u05d1\u05d9\u05df \u05d4\u05e2\u05dc\u05d9\u05d5\u05ea \u05d4\u05d1\u05d5\u05dc\u05d8\u05d5\u05ea \u05d1\u05de\u05e0\u05d9\u05d5\u05ea \u05d4\u05d1\u05e0\u05e7\u05d9\u05dd - \u05e7\u05d5\u05de\u05e8\u05e6\u05d1\u05e0\u05e7 \u05e7\u05d5\u05e4\u05e5 \u05d1-8%, \u05d3\u05d5\u05d9\u05d8\u05e9\u05d4 \u05d1\u05e0\u05e7 \u05de\u05ea\u05d7\u05d6\u05e7 \u05d1-8.5%, \u05e7\u05e8\u05d3\u05d9\u05d8 \u05e1\u05d5\u05d5\u05d9\u05e1 \u05e2\u05d5\u05dc\u05d4 \u05d1-6.8%, UBS \u05de\u05d5\u05e1\u05d9\u05e3 5.9%.\r\n \r\n \r\n \r\n9:40 - \u05d1\u05d5\u05e8\u05e1\u05ea \u05d8\u05d5\u05e7\u05d9\u05d5 \u05e0\u05e0\u05e2\u05dc\u05d4 \u05d1\u05d6\u05d9\u05e0\u05d5\u05e7 \u05e9\u05dc 7%, \u05e9\u05e0\u05d2\u05d7\u05d0\u05d9 \u05d4\u05ea\u05d7\u05d6\u05e7\u05d4 \u05d1-2.3% \u05d5\u05e1\u05d9\u05d0\u05d5\u05dc \u05e7\u05e4\u05e6\u05d4 \u05d1-8.6%. \u05d1\u05d1\u05d5\u05e8\u05e1\u05ea \u05d4\u05d5\u05e0\u05d2 \u05e7\u05d5\u05e0\u05d2 \u05e2\u05d3\u05d9\u05d9\u05df \u05de\u05ea\u05e7\u05d9\u05d9\u05dd \u05d4\u05de\u05e1\u05d7\u05e8 \u05d5\u05de\u05d3\u05d3 \u05d4\u05d0\u05e0\u05d2 \u05e1\u05e0\u05d2 \u05de\u05d8\u05e4\u05e1 \u05d1-4.2%.\r\n \r\n7:30 - \u05d4\u05d1\u05d5\u05e8\u05e1\u05d5\u05ea \u05d1\u05d0\u05e1\u05d9\u05d4 \u05e8\u05d5\u05e9\u05de\u05d5\u05ea \u05d4\u05d1\u05d5\u05e7\u05e8 \u05e2\u05dc\u05d9\u05d5\u05ea \u05d7\u05d3\u05d5\u05ea - \u05de\u05d3\u05d3 \u05e0\u05d9\u05e7\u05d9\u05d9 \u05d1\u05d8\u05d5\u05e7\u05d9\u05d5 \u05e7\u05d5\u05e4\u05e5 \u05d1-6.3%, \u05d4\u05d0\u05e0\u05d2 \u05e1\u05e0\u05d2 \u05d1\u05d4\u05d5\u05e0\u05d2 \u05e7\u05d5\u05e0\u05d2 \u05de\u05ea\u05d7\u05d6\u05e7 \u05d1-3.4%, \u05de\u05d3\u05d3 \u05e7\u05d5\u05e1\u05e4\u05d9 \u05d1\u05e1\u05d9\u05d0\u05d5\u05dc \u05de\u05d6\u05e0\u05e7 \u05d1-6.1% \u05d5\u05de\u05d3\u05d3 \u05e9\u05e0\u05d2\u05d7\u05d0\u05d9 \u05e2\u05d5\u05dc\u05d4 \u05d1-0.9%. \u05d4\u05e2\u05e0\u05e7\u05d9\u05ea \u05d4\u05d9\u05e4\u05e0\u05d9\u05ea \u05e1\u05d5\u05e4\u05d8\u05d1\u05e0\u05e7 \u05de\u05de\u05e8\u05d9\u05d0\u05d4 \u05d1-19.5% \u05d1\u05d4\u05de\u05e9\u05da \u05dc\u05d6\u05d9\u05e0\u05d5\u05e7 \u05e9\u05dc 18.6% \u05d0\u05ea\u05de\u05d5\u05dc, \u05dc\u05d0\u05d7\u05e8 \u05e9\u05d4\u05d5\u05d3\u05d9\u05e2\u05d4 \u05e2\u05dc \u05ea\u05d5\u05db\u05e0\u05d9\u05ea \u05e2\u05e0\u05e7\u05d9\u05ea \u05dc\u05de\u05db\u05d9\u05e8\u05ea \u05e0\u05db\u05e1\u05d9\u05dd \u05d1\u05e9\u05d5\u05d5\u05d9 41 \u05de\u05d9\u05dc\u05d9\u05d5\u05df \u05d3\u05d5\u05dc\u05e8 \u05d5\u05e8\u05db\u05d9\u05e9\u05d4 \u05d7\u05d5\u05d6\u05e8\u05ea \u05e9\u05dc \u05de\u05e0\u05d9\u05d5\u05ea.\r\n \r\n\u05d4\u05d7\u05d5\u05d6\u05d9\u05dd \u05d4\u05e2\u05ea\u05d9\u05d3\u05d9\u05d9\u05dd \u05e2\u05dc \u05d4\u05de\u05d3\u05d3\u05d9\u05dd \u05d4\u05de\u05d5\u05d1\u05d9\u05dc\u05d9\u05dd \u05d1\u05e0\u05d9\u05d5 \u05d9\u05d5\u05e8\u05e7 \u05e7\u05d5\u05e4\u05e6\u05d9\u05dd \u05d1-2.5% \u05e2\u05d3 3% \u05db\u05e9\u05d1\u05e8\u05e7\u05e2 \u05d7\u05d1\u05e8\u05d9 \u05d4\u05e1\u05e0\u05d0\u05d8 \u05de\u05de\u05e9\u05d9\u05db\u05d9\u05dd \u05dc\u05e0\u05e1\u05d5\u05ea \u05dc\u05de\u05e6\u05d5\u05d0 \u05d4\u05e6\u05e2\u05ea \u05d7\u05d5\u05e7 \u05e9\u05ea\u05e7\u05dc \u05e2\u05dc \u05d4\u05db\u05dc\u05db\u05dc\u05d4 \u05d4\u05d0\u05de\u05e8\u05d9\u05e7\u05d0\u05d9\u05ea.\r\n \r\n\u05d4\u05d1\u05e0\u05e7 \u05d4\u05de\u05e8\u05db\u05d6\u05d9 \u05e9\u05dc \u05d0\u05e8\u05d4&quot;\u05d1, \u05d4\u05e4\u05d3\u05e8\u05dc \u05e8\u05d9\u05d6\u05e8\u05d1, \u05d4\u05d5\u05d3\u05d9\u05e2 \u05d0\u05ea\u05de\u05d5\u05dc \u05e2\u05dc \u05ea\u05d5\u05db\u05e0\u05d9\u05ea \u05d4\u05e7\u05dc\u05d4 \u05db\u05de\u05d5\u05ea\u05d9\u05ea \u05e9\u05d1\u05de\u05e1\u05d2\u05e8\u05ea\u05d4 \u05d9\u05e8\u05db\u05d5\u05e9 \u05e0\u05db\u05e1\u05d9\u05dd \u05d1\u05db\u05de\u05d5\u05ea \u05d1\u05dc\u05ea\u05d9 \u05de\u05d5\u05d2\u05d1\u05dc\u05ea. \u05d1\u05e4